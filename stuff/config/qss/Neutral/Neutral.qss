* {
  padding: 0;
  margin: 0;
  outline: 0;
  border: 0;
  border-style: none;
  border-image: none;
}
QToolBar * {
  padding: 0;
  margin: 0;
}
:TOONZCOLORS {
  -bg-color: #808080;
  -icon-base-opacity: 0.8;
  -icon-disabled-opacity: 0.3;
  -icon-base-color: black;
  -icon-active-color: black;
  -icon-selected-color: black;
  -icon-on-color: black;
  -icon-close-color: black;
  -icon-preview-color: black;
  -icon-vcheck-color: black;
  -icon-lock-color: black;
  -icon-keyframe-color: black;
  -icon-keyframe-modified-color: black;
}
QWidget {
  background-color: #808080;
  color: black;
}
QWidget:disabled {
  color: rgba(0, 0, 0, 0.4);
}
/* -----------------------------------------------------------------------------
   MainWindow & Floating Panels
----------------------------------------------------------------------------- */
QFrame {
  border: 0;
  margin: 0;
  padding: 0;
}
QToolTip,
#helpToolTip {
  background-color: #181818;
  border: 1 solid #828282;
  color: #d2d2d2;
  padding: 2px;
}
#DockSeparator,
QMainWindow::separator,
QSplitter::handle {
  background-color: #292929;
  height: 4;
  width: 4;
}
#TDockPlaceholder {
  background-color: #1aff00;
}
TPanel {
  background-color: #292929;
}
/* -----------------------------------------------------------------------------
   Topbar
----------------------------------------------------------------------------- */
#TopBar {
  background-color: #808080;
  border: 0;
  border-bottom: 1 solid #4f4f4f;
  margin: 0;
}
#TopBar #EditToolLockButton {
  background-color: #808080;
  spacing: 0;
}
#TopBar #EditToolLockButton::indicator {
  background: none;
  border: none;
  padding-left: 0;
  padding-right: 0;
}
#TopBarTabContainer {
  background-color: #808080;
  margin-bottom: 1;
}
#StackedMenuBar {
  border: 0;
  margin: 0;
  padding: 0;
}
QMenuBar {
  background-color: #808080;
  border: 0;
}
QMenuBar::item {
  background-color: #808080;
  border-left: 1 solid #808080;
  padding: 3 5;
}
QMenuBar::item:selected {
  background-color: #989898;
  color: #000000;
}
QMenuBar::item:pressed {
  background-color: #ced7d8;
  color: #000000;
}
/* -----------------------------------------------------------------------------
   Rooms Workspaces
----------------------------------------------------------------------------- */
#TopBarTab {
  margin: 0;
  padding: 0;
  border: 0;
  background-color: transparent;
}
#TopBarTab::tab {
  background-color: #6e6e6e;
  padding: 2 8 3 8;
  margin: 0 -1 0 0;
  border-color: #4f4f4f;
  border-radius: 0;
  border-bottom-width: 0;
}
#TopBarTab::tab:hover {
  background-color: #878787;
}
#TopBarTab::tab:selected {
  background-color: #ced7d8;
  border-color: #4f4f4f;
  color: #000000;
}
#TopBarTab::tab:selected:hover {
  background-color: #e9edee;
}
#TopBarTab::tab:first {
  border-left-width: 1;
  border-top-left-radius: 2;
}
#TopBarTab::tab:last {
  margin-right: 0;
  border-top-right-radius: 2;
}
#TopBarTab::tab:only-one {
  border-left-width: 1;
}
#TopBarTab QToolButton {
  border-left: 2 solid #4f4f4f;
  border-top: 1 solid #4f4f4f;
  border-right: 1 solid #4f4f4f;
  border-bottom: 0;
  margin-left: -1;
  margin-top: 0;
}
/* -----------------------------------------------------------------------------
   Context Menu
----------------------------------------------------------------------------- */
QMenu {
  background-color: #9a9a9a;
  border: 1 solid #4f4f4f;
  color: black;
  padding: 2 0;
}
QMenu::item {
  border: 0;
  padding: 3 28 3 14;
}
QMenu::item:selected {
  background-color: #ced7d8;
  color: #000000;
}
QMenu::item:checked {
  color: #0f0f86;
  background-color: #9a9a9a;
  font-weight: bold;
}
QMenu::item:checked:selected {
  background-color: #ced7d8;
  color: #000000;
}
QMenu::item:disabled {
  background: none;
  color: rgba(0, 0, 0, 0.4);
}
QMenu::item:disabled:selected {
  color: rgba(0, 0, 0, 0.4);
}
QMenu::separator {
  border-top: 1 solid #4f4f4f;
  height: 0;
  margin: 2 0;
}
QMenu::icon {
  border-radius: 2;
  position: absolute;
  left: 7;
  padding: 0;
  margin: 0;
  qproperty-icon: url('../Default/imgs/black/misc/transparent_16x16.png');
}
QMenu::icon:checked {
  background-color: #ced7d8;
  border: 1 solid #ced7d8;
}
QMenu::indicator {
  margin-left: 8;
}
QMenu::indicator:non-exclusive {
  background-color: #c7c7c7;
  border: 1 solid #4f4f4f;
  border-radius: 2px;
  padding: 1;
  width: 10px;
  height: 10px;
  border-color: #4f4f4f;
}
QMenu::indicator:non-exclusive:hover {
  background-color: #e0e0e0;
  border-color: #4f4f4f;
}
QMenu::indicator:non-exclusive:checked {
  background-color: black;
  border-color: black;
  image: url('../Default/imgs/black/checkmark/checkmark.svg');
}
QMenu::indicator:non-exclusive:checked:hover {
  background-color: #3d3d3d;
  border-color: #3d3d3d;
}
QMenu::indicator:non-exclusive:checked:disabled {
  background-color: #333333;
  border-color: #333333;
  image: url('../Default/imgs/black/checkmark/checkmark_disabled.svg');
}
QMenu::indicator:non-exclusive:indeterminate {
  background-color: black;
  border-color: black;
  image: url('../Default/imgs/black/checkmark/checkpartially.svg');
}
QMenu::indicator:non-exclusive:indeterminate:hover {
  background-color: #e0e0e0;
  border-color: #4f4f4f;
}
QMenu::indicator:non-exclusive:indeterminate:disabled {
  background-color: #4d4d4d;
  border-color: #4d4d4d;
  image: url('../Default/imgs/black/checkmark/checkpartially_disabled.svg');
}
QMenu::indicator:non-exclusive:disabled {
  background-color: #9d9d9d;
  border-color: #6c6c6c;
}
QMenu::indicator:non-exclusive:selected {
  border-color: #4f4f4f;
}
QMenu::indicator:non-exclusive:checked {
  border-color: black;
}
QMenu::indicator:non-exclusive:checked:selected {
  border-color: black;
}
QMenu::indicator:exclusive {
  width: 13;
  height: 13;
  background-color: transparent;
  border: 0;
  image-position: center center;
}
QMenu::indicator:exclusive:unchecked {
  image: url('../Default/imgs/black/radiobutton/radiobutton_unchecked.svg');
}
QMenu::indicator:exclusive:unchecked:hover {
  image: url('../Default/imgs/black/radiobutton/radiobutton_unchecked_hover.svg');
}
QMenu::indicator:exclusive:checked {
  image: url('../Default/imgs/black/radiobutton/radiobutton_checked.svg');
}
QMenu::indicator:exclusive:checked:disabled {
  background-color: transparent;
  image: url('../Default/imgs/black/radiobutton/radiobutton_checked_disabled.svg');
}
QMenu::indicator:exclusive:disabled {
  image: url('../Default/imgs/black/radiobutton/radiobutton_unchecked_disabled.svg');
}
/* -----------------------------------------------------------------------------
   Titlebars
----------------------------------------------------------------------------- */
TPanelTitleBar {
  background-color: #6e6e6e;
  border-bottom: 1 solid #4f4f4f;
  border-top-left-radius: 0px;
  border-top-right-radius: 0px;
  height: 18;
  min-height: 18;
  qproperty-TitleColor: black;
  qproperty-ActiveTitleColor: #e9e9e9;
  qproperty-CloseOverColor: #f16a6a;
  font-weight: bold;
}
TPanelTitleBar:hover {
  background-color: #6e6e6e;
}
TPanelTitleBarButton {
  qproperty-OffColor: #6e6e6e;
  qproperty-OverColor: #9c9c9c;
  qproperty-PressedColor: #ced7d8;
  qproperty-FreezeColor: #f16a6a;
  qproperty-PreviewColor: #d2cb67;
}
/* -----------------------------------------------------------------------------
   Scrollbars
----------------------------------------------------------------------------- */
QAbstractScrollArea::corner {
  background-color: #8d8d8d;
}
QScrollBar {
  background-color: #8d8d8d;
  border: 0;
}
QScrollBar:horizontal {
  height: 15;
  margin: 0;
}
QScrollBar:vertical {
  margin: 0;
  width: 15;
}
QScrollBar::handle {
  background-color: #5f5f5f;
  border-style: solid;
  border-width: 1;
  border-color: #5f5f5f;
  border-radius: 5px;
}
QScrollBar::handle:horizontal:hover,
QScrollBar::handle:vertical:hover {
  background-color: #464646;
  border-color: #464646;
}
QScrollBar::handle:horizontal:pressed,
QScrollBar::handle:vertical:pressed {
  background-color: #2e2e2e;
  border-color: #2e2e2e;
}
QScrollBar::handle:horizontal {
  margin: 2px 16;
  min-width: 20;
}
QScrollBar::handle:vertical {
  margin: 16 2px;
  min-height: 20;
}
QScrollBar::add-line {
  subcontrol-origin: margin;
  border: 0;
}
QScrollBar::add-line:horizontal {
  subcontrol-position: right;
  background-color: #8d8d8d;
  margin: 0;
  width: 16;
}
QScrollBar::add-line:vertical {
  subcontrol-position: bottom;
  background-color: #8d8d8d;
  margin: 0;
  height: 16;
}
QScrollBar::sub-line {
  border: 0;
  subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
  subcontrol-position: left;
  background-color: #8d8d8d;
  margin: 0;
  width: 16;
}
QScrollBar::sub-line:vertical {
  subcontrol-position: top;
  background-color: #8d8d8d;
  margin: 0;
  height: 16;
}
QScrollBar::up-arrow:vertical {
  image: url('../Default/imgs/black/scrollbar/scroll-up.svg');
  image-position: center center;
}
QScrollBar::up-arrow:vertical:hover {
  image: url('../Default/imgs/black/scrollbar/scroll-up-hover.svg');
}
QScrollBar::up-arrow:vertical:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/black/scrollbar/scroll-up-hover.svg');
}
QScrollBar::down-arrow:vertical {
  image: url('../Default/imgs/black/scrollbar/scroll-down.svg');
  image-position: center center;
}
QScrollBar::down-arrow:vertical:hover {
  image: url('../Default/imgs/black/scrollbar/scroll-down-hover.svg');
}
QScrollBar::down-arrow:vertical:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/black/scrollbar/scroll-down-hover.svg');
}
QScrollBar::left-arrow:horizontal {
  image: url('../Default/imgs/black/scrollbar/scroll-left.svg');
  image-position: center center;
}
QScrollBar::left-arrow:horizontal:hover {
  image: url('../Default/imgs/black/scrollbar/scroll-left-hover.svg');
}
QScrollBar::left-arrow:horizontal:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/black/scrollbar/scroll-left-hover.svg');
}
QScrollBar::right-arrow:horizontal {
  image: url('../Default/imgs/black/scrollbar/scroll-right.svg');
  image-position: center center;
}
QScrollBar::right-arrow:horizontal:hover {
  image: url('../Default/imgs/black/scrollbar/scroll-right-hover.svg');
}
QScrollBar::right-arrow:horizontal:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/black/scrollbar/scroll-right.svg');
}
QScrollBar::sub-page:horizontal,
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:vertical,
QScrollBar::add-page:vertical {
  background: none;
}
/* -----------------------------------------------------------------------------
   Progress Bar
----------------------------------------------------------------------------- */
QProgressBar {
  text-align: center;
  background-color: #727272;
  border: 1 solid #4f4f4f;
  border-radius: 3;
  padding: 0;
}
QProgressBar::chunk {
  margin: -1;
  background-color: #399739;
  border: 1 solid #4f4f4f;
  border-radius: 2;
}
/* -----------------------------------------------------------------------------
   Buttons
----------------------------------------------------------------------------- */
QPushButton {
  background-color: #9a9a9a;
  border-color: #5c5c5c;
  border-width: 1;
  border-style: solid;
  border-radius: 2px;
  color: black;
  margin: 0;
  padding: 2px 12px;
}
QPushButton:hover {
  background-color: #b2b2b2;
  border-color: #5c5c5c;
  color: black;
}
QPushButton:pressed {
  background-color: #686868;
  border-color: #3e3e3e;
  color: black;
}
QPushButton:checked {
  background-color: #686868;
  border-color: #3e3e3e;
  color: #e9e9e9;
}
QPushButton:checked:hover {
  background-color: #808080;
  border-color: #3e3e3e;
}
QPushButton:checked:pressed {
  background-color: #686868;
}
QPushButton:disabled {
  background-color: #8f8f8f;
  border-color: #727272;
  color: rgba(0, 0, 0, 0.4);
}
#PushButton_NoPadding {
  padding: 3;
}
#CustomPanelButton {
  background-color: #9a9a9a;
  border-color: #5c5c5c;
  border-width: 1;
  border-style: solid;
  border-radius: 2px;
  color: black;
  margin: 0;
  padding: 2px 12px;
  padding: 0;
}
#CustomPanelButton:hover {
  background-color: #b2b2b2;
  border-color: #5c5c5c;
  color: black;
}
#CustomPanelButton:pressed {
  background-color: #686868;
  border-color: #3e3e3e;
  color: black;
}
#CustomPanelButton:checked {
  background-color: #686868;
  border-color: #3e3e3e;
  color: #e9e9e9;
}
#CustomPanelButton:checked:hover {
  background-color: #808080;
  border-color: #3e3e3e;
}
#CustomPanelButton:checked:pressed {
  background-color: #686868;
}
#CustomPanelButton:disabled {
  background-color: #8f8f8f;
  border-color: #727272;
  color: rgba(0, 0, 0, 0.4);
}
/* -----------------------------------------------------------------------------
   ComboBox
----------------------------------------------------------------------------- */
QComboBox {
  background-color: #9a9a9a;
  border-color: #5c5c5c;
  border-width: 1;
  border-style: solid;
  border-radius: 2px;
  color: black;
  margin: 0;
  padding: 1 0 1 4;
}
QComboBox:hover {
  background-color: #b2b2b2;
  color: black;
  border-color: #5c5c5c;
}
QComboBox:checked {
  border-color: black;
}
QComboBox:pressed {
  background-color: #9a9a9a;
  border-color: black;
  color: black;
}
QComboBox:pressed:focus {
  background-color: #9a9a9a;
  border-color: black;
}
QComboBox:disabled {
  background-color: #8f8f8f;
  border-color: #727272;
  color: rgba(0, 0, 0, 0.4);
}
QComboBox:focus {
  background-color: #9a9a9a;
  border-color: black;
}
QComboBox:focus:hover {
  background-color: #b2b2b2;
}
QComboBox:editable {
  color: black;
  background: none;
  background-color: #c7c7c7;
  border-color: #4f4f4f;
  selection-background-color: #0078D7;
  selection-color: white;
  padding: 1 0 1 3;
}
QComboBox:editable:hover {
  background-color: #c7c7c7;
  border-color: #4f4f4f;
}
QComboBox:editable:checked {
  border-color: black;
}
QComboBox:editable:focus {
  border-color: black;
}
QComboBox::drop-down {
  border: 0 solid transparent;
  image: url('../Default/imgs/black/combobox/combo_downarrow.svg');
  image-position: center center;
  width: 16;
}
QComboBox::drop-down:disabled {
  image: url('../Default/imgs/black/combobox/combo_downarrow_disabled.svg');
}
QComboBox::drop-down:disabled:editable {
  background-color: #8f8f8f;
}
QComboBox::drop-down:editable {
  background-color: #9a9a9a;
  border-left: 1 solid #4f4f4f;
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
}
QComboBox::drop-down:editable:hover {
  background-color: #b2b2b2;
}
QComboBox QAbstractItemView {
  background-color: #9a9a9a;
  selection-background-color: #ced7d8;
  selection-color: #000000;
  border: 1 solid #4f4f4f;
}
/* -----------------------------------------------------------------------------
   Textfields
----------------------------------------------------------------------------- */
QPlainTextEdit,
QLineEdit,
QTextEdit[readOnly="false"] {
  background-color: #c7c7c7;
  border-style: solid;
  border-width: 1;
  border-color: #4f4f4f;
  border-radius: 2px;
  color: black;
  selection-background-color: #0078D7;
  selection-color: white;
  padding: 1 0 1 1;
}
QPlainTextEdit:hover,
QLineEdit:hover,
QTextEdit[readOnly="false"]:hover {
  background-color: #c7c7c7;
  color: black;
  border-color: #4f4f4f;
}
QPlainTextEdit:focus,
QLineEdit:focus,
QTextEdit[readOnly="false"]:focus {
  background-color: #c7c7c7;
  border-color: black;
  color: black;
}
QPlainTextEdit:disabled,
QLineEdit:disabled,
QTextEdit[readOnly="false"]:disabled {
  background: none;
  color: #5e5e5e;
  background-color: #9d9d9d;
  border-color: rgba(79, 79, 79, 0.4);
}
QTextEdit[readOnly="true"] {
  background-color: #808080;
  color: black;
  border: 0;
}
QTextEdit[readOnly="true"]:focus,
QTextEdit[readOnly="true"]:disabled {
  background-color: #808080;
  color: black;
  border: 0;
}
/* -----------------------------------------------------------------------------
   Options
----------------------------------------------------------------------------- */
QCheckBox {
  color: black;
  padding: 0 2 0 0;
  border-radius: 2;
}
QCheckBox:hover {
  background-color: #989898;
  color: black;
}
QCheckBox:disabled {
  color: rgba(0, 0, 0, 0.4);
}
QCheckBox::indicator {
  background-color: #c7c7c7;
  border: 1 solid #4f4f4f;
  border-radius: 2px;
  padding: 1;
  width: 10px;
  height: 10px;
}
QCheckBox::indicator:hover {
  background-color: #e0e0e0;
  border-color: #4f4f4f;
}
QCheckBox::indicator:checked {
  background-color: black;
  border-color: black;
  image: url('../Default/imgs/black/checkmark/checkmark.svg');
}
QCheckBox::indicator:checked:hover {
  background-color: #3d3d3d;
  border-color: #3d3d3d;
}
QCheckBox::indicator:checked:disabled {
  background-color: #333333;
  border-color: #333333;
  image: url('../Default/imgs/black/checkmark/checkmark_disabled.svg');
}
QCheckBox::indicator:indeterminate {
  background-color: black;
  border-color: black;
  image: url('../Default/imgs/black/checkmark/checkpartially.svg');
}
QCheckBox::indicator:indeterminate:hover {
  background-color: #e0e0e0;
  border-color: #4f4f4f;
}
QCheckBox::indicator:indeterminate:disabled {
  background-color: #4d4d4d;
  border-color: #4d4d4d;
  image: url('../Default/imgs/black/checkmark/checkpartially_disabled.svg');
}
QCheckBox::indicator:disabled {
  background-color: #9d9d9d;
  border-color: #6c6c6c;
}
QRadioButton {
  color: black;
}
QRadioButton:hover {
  color: black;
}
QRadioButton:checked {
  color: black;
}
QRadioButton:disabled {
  color: rgba(0, 0, 0, 0.4);
}
QRadioButton::indicator {
  width: 13;
  height: 13;
  background-color: transparent;
  border: 0;
  image-position: center center;
}
QRadioButton::indicator:unchecked {
  image: url('../Default/imgs/black/radiobutton/radiobutton_unchecked.svg');
}
QRadioButton::indicator:unchecked:hover {
  image: url('../Default/imgs/black/radiobutton/radiobutton_unchecked_hover.svg');
}
QRadioButton::indicator:checked {
  image: url('../Default/imgs/black/radiobutton/radiobutton_checked.svg');
}
QRadioButton::indicator:checked:disabled {
  background-color: transparent;
  image: url('../Default/imgs/black/radiobutton/radiobutton_checked_disabled.svg');
}
QRadioButton::indicator:disabled {
  image: url('../Default/imgs/black/radiobutton/radiobutton_unchecked_disabled.svg');
}
/* -----------------------------------------------------------------------------
   GroupBox
----------------------------------------------------------------------------- */
QGroupBox {
  background-color: transparent;
  border-width: 1;
  border-style: solid;
  border-radius: 2px;
  border-color: #4f4f4f;
  margin: 6 0 0 0;
  padding: 5 5;
}
QGroupBox::title {
  subcontrol-origin: margin;
  color: black;
  left: 15;
  margin: -2 0 0 0;
  padding: 0 3;
  border-radius: 2;
}
QGroupBox::title:hover {
  color: black;
  background-color: #989898;
}
QGroupBox::title:disabled {
  color: rgba(0, 0, 0, 0.4);
}
QGroupBox::indicator {
  background-color: #c7c7c7;
  border: 1 solid #4f4f4f;
  border-radius: 2px;
  padding: 1;
  width: 10px;
  height: 10px;
  subcontrol-origin: margin;
  margin-top: 2;
}
QGroupBox::indicator:hover {
  background-color: #e0e0e0;
  border-color: #4f4f4f;
}
QGroupBox::indicator:checked {
  background-color: black;
  border-color: black;
  image: url('../Default/imgs/black/checkmark/checkmark.svg');
}
QGroupBox::indicator:checked:hover {
  background-color: #3d3d3d;
  border-color: #3d3d3d;
}
QGroupBox::indicator:checked:disabled {
  background-color: #333333;
  border-color: #333333;
  image: url('../Default/imgs/black/checkmark/checkmark_disabled.svg');
}
QGroupBox::indicator:indeterminate {
  background-color: black;
  border-color: black;
  image: url('../Default/imgs/black/checkmark/checkpartially.svg');
}
QGroupBox::indicator:indeterminate:hover {
  background-color: #e0e0e0;
  border-color: #4f4f4f;
}
QGroupBox::indicator:indeterminate:disabled {
  background-color: #4d4d4d;
  border-color: #4d4d4d;
  image: url('../Default/imgs/black/checkmark/checkpartially_disabled.svg');
}
