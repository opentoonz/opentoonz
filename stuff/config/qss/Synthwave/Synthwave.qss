* {
  padding: 0;
  margin: 0;
  outline: 0;
  border: 0;
  border-style: none;
  border-image: none;
}
QToolBar * {
  padding: 0;
  margin: 0;
}
:TOONZCOLORS {
  -bg-color: #2b313a;
  -icon-base-opacity: 0.8;
  -icon-disabled-opacity: 0.3;
  -icon-base-color: #cecece;
  -icon-active-color: white;
  -icon-selected-color: white;
  -icon-on-color: white;
  -icon-close-color: white;
  -icon-preview-color: black;
  -icon-vcheck-color: white;
  -icon-lock-color: white;
  -icon-keyframe-color: white;
  -icon-keyframe-modified-color: white;
}
QWidget {
  background-color: #2b313a;
  color: #d3d3d3;
}
QWidget:disabled {
  color: rgba(211, 211, 211, 0.4);
}
/* -----------------------------------------------------------------------------
   MainWindow & Floating Panels
----------------------------------------------------------------------------- */
QFrame {
  border: 0;
  margin: 0;
  padding: 0;
}
QToolTip,
#helpToolTip {
  background-color: #181818;
  border: 1 solid #828282;
  color: #d2d2d2;
  padding: 2px;
}
#DockSeparator,
QMainWindow::separator,
QSplitter::handle {
  background-color: #000000;
  height: 4;
  width: 4;
}
#TDockPlaceholder {
  background-color: #1aff00;
}
TPanel {
  background-color: #000000;
}
/* -----------------------------------------------------------------------------
   Topbar
----------------------------------------------------------------------------- */
#TopBar {
  background-color: #2b313a;
  border: 0;
  border-bottom: 1 solid #010102;
  margin: 0;
}
#TopBar #EditToolLockButton {
  background-color: #2b313a;
  spacing: 0;
}
#TopBar #EditToolLockButton::indicator {
  background: none;
  border: none;
  padding-left: 0;
  padding-right: 0;
}
#TopBarTabContainer {
  background-color: #2b313a;
  margin-bottom: 1;
}
#StackedMenuBar {
  border: 0;
  margin: 0;
  padding: 0;
}
QMenuBar {
  background-color: #2b313a;
  border: 0;
}
QMenuBar::item {
  background-color: #2b313a;
  border-left: 1 solid #2b313a;
  padding: 3 5;
}
QMenuBar::item:selected {
  background-color: #3c4551;
  color: #ffffff;
}
QMenuBar::item:pressed {
  background-color: #aa4ca2;
  color: #ffffff;
}
/* -----------------------------------------------------------------------------
   Rooms Workspaces
----------------------------------------------------------------------------- */
#TopBarTab {
  margin: 0;
  padding: 0;
  border: 0;
  background-color: transparent;
}
#TopBarTab::tab {
  background-color: #22272e;
  padding: 2 8 3 8;
  margin: 0 -1 0 0;
  border-color: #010102;
  border-radius: 0;
  border-bottom-width: 0;
}
#TopBarTab::tab:hover {
  background-color: #343b46;
}
#TopBarTab::tab:selected {
  background-color: #1cc6c6;
  border-color: #010102;
  color: black;
}
#TopBarTab::tab:selected:hover {
  background-color: #2ae1e1;
}
#TopBarTab::tab:first {
  border-left-width: 1;
  border-top-left-radius: 2;
}
#TopBarTab::tab:last {
  margin-right: 0;
  border-top-right-radius: 2;
}
#TopBarTab::tab:only-one {
  border-left-width: 1;
}
#TopBarTab QToolButton {
  border-left: 2 solid #010102;
  border-top: 1 solid #010102;
  border-right: 1 solid #010102;
  border-bottom: 0;
  margin-left: -1;
  margin-top: 0;
}
/* -----------------------------------------------------------------------------
   Context Menu
----------------------------------------------------------------------------- */
QMenu {
  background-color: #2b313a;
  border: 1 solid #010102;
  color: #d3d3d3;
  padding: 2 0;
}
QMenu::item {
  border: 0;
  padding: 3 28 3 14;
}
QMenu::item:selected {
  background-color: #aa4ca2;
  color: #ffffff;
}
QMenu::item:checked {
  color: yellow;
  background-color: #2b313a;
  font-weight: bold;
}
QMenu::item:checked:selected {
  background-color: #aa4ca2;
  color: #ffffff;
}
QMenu::item:disabled {
  background: none;
  color: rgba(211, 211, 211, 0.4);
}
QMenu::item:disabled:selected {
  color: rgba(211, 211, 211, 0.4);
}
QMenu::separator {
  border-top: 1 solid #010102;
  height: 0;
  margin: 2 0;
}
QMenu::icon {
  border-radius: 2;
  position: absolute;
  left: 7;
  padding: 0;
  margin: 0;
  qproperty-icon: url('../Default/imgs/white/misc/transparent_16x16.png');
}
QMenu::icon:checked {
  background-color: #aa4ca2;
  border: 1 solid #aa4ca2;
}
QMenu::indicator {
  margin-left: 8;
}
QMenu::indicator:non-exclusive {
  background-color: #131519;
  border: 1 solid #010102;
  border-radius: 2px;
  padding: 1;
  width: 10px;
  height: 10px;
  border-color: #010102;
}
QMenu::indicator:non-exclusive:hover {
  background-color: #242931;
  border-color: #242931;
}
QMenu::indicator:non-exclusive:checked {
  background-color: #d8d80f;
  border-color: #d8d80f;
  image: url('../Default/imgs/white/checkmark/checkmark_black.svg');
}
QMenu::indicator:non-exclusive:checked:hover {
  background-color: #f0f020;
  border-color: #f0f020;
}
QMenu::indicator:non-exclusive:checked:disabled {
  background-color: #939520;
  border-color: #939520;
  image: url('../Default/imgs/white/checkmark/checkmark_disabled_black.svg');
}
QMenu::indicator:non-exclusive:indeterminate {
  background-color: #d8d80f;
  border-color: #d8d80f;
  image: url('../Default/imgs/white/checkmark/checkpartially_black.svg');
}
QMenu::indicator:non-exclusive:indeterminate:hover {
  background-color: #242931;
  border-color: #242931;
}
QMenu::indicator:non-exclusive:indeterminate:disabled {
  background-color: #707429;
  border-color: #707429;
  image: url('../Default/imgs/white/checkmark/checkpartially_disabled_black.svg');
}
QMenu::indicator:non-exclusive:disabled {
  background-color: #21262d;
  border-color: #1a1e24;
}
QMenu::indicator:non-exclusive:selected {
  border-color: #131519;
}
QMenu::indicator:non-exclusive:checked {
  border-color: #d8d80f;
}
QMenu::indicator:non-exclusive:checked:selected {
  border-color: #d8d80f;
}
QMenu::indicator:exclusive {
  width: 13;
  height: 13;
  background-color: transparent;
  border: 0;
  image-position: center center;
}
QMenu::indicator:exclusive:unchecked {
  image: url('../Default/imgs/white/radiobutton/radiobutton_unchecked.svg');
}
QMenu::indicator:exclusive:unchecked:hover {
  image: url('../Default/imgs/white/radiobutton/radiobutton_unchecked_hover.svg');
}
QMenu::indicator:exclusive:checked {
  image: url('../Default/imgs/white/radiobutton/radiobutton_checked.svg');
}
QMenu::indicator:exclusive:checked:disabled {
  background-color: transparent;
  image: url('../Default/imgs/white/radiobutton/radiobutton_checked_disabled.svg');
}
QMenu::indicator:exclusive:disabled {
  image: url('../Default/imgs/white/radiobutton/radiobutton_unchecked_disabled.svg');
}
/* -----------------------------------------------------------------------------
   Titlebars
----------------------------------------------------------------------------- */
TPanelTitleBar {
  background-color: #22272e;
  border-bottom: 1 solid #010102;
  border-top-left-radius: 0px;
  border-top-right-radius: 0px;
  height: 18;
  min-height: 18;
  qproperty-TitleColor: rgba(211, 211, 211, 0.6);
  qproperty-ActiveTitleColor: #e4e410;
  qproperty-CloseOverColor: #d44a4a;
  font-weight: bold;
}
TPanelTitleBar:hover {
  background-color: #22272e;
}
TPanelTitleBarButton {
  qproperty-OffColor: #22272e;
  qproperty-OverColor: #434c5a;
  qproperty-PressedColor: #aa4ca2;
  qproperty-FreezeColor: #d44a4a;
  qproperty-PreviewColor: #61bfbd;
}
/* -----------------------------------------------------------------------------
   Scrollbars
----------------------------------------------------------------------------- */
QAbstractScrollArea::corner {
  background-color: #1d2127;
}
QScrollBar {
  background-color: #1d2127;
  border: 0;
}
QScrollBar:horizontal {
  height: 15;
  margin: 0;
}
QScrollBar:vertical {
  margin: 0;
  width: 15;
}
QScrollBar::handle {
  background-color: #414a57;
  border-style: solid;
  border-width: 1;
  border-color: #414a57;
  border-radius: 5px;
}
QScrollBar::handle:horizontal:hover,
QScrollBar::handle:vertical:hover {
  background-color: #525e6f;
  border-color: #525e6f;
}
QScrollBar::handle:horizontal:pressed,
QScrollBar::handle:vertical:pressed {
  background-color: #637186;
  border-color: #637186;
}
QScrollBar::handle:horizontal {
  margin: 2px 16;
  min-width: 20;
}
QScrollBar::handle:vertical {
  margin: 16 2px;
  min-height: 20;
}
QScrollBar::add-line {
  subcontrol-origin: margin;
  border: 0;
}
QScrollBar::add-line:horizontal {
  subcontrol-position: right;
  background-color: #1d2127;
  margin: 0;
  width: 16;
}
QScrollBar::add-line:vertical {
  subcontrol-position: bottom;
  background-color: #1d2127;
  margin: 0;
  height: 16;
}
QScrollBar::sub-line {
  border: 0;
  subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
  subcontrol-position: left;
  background-color: #1d2127;
  margin: 0;
  width: 16;
}
QScrollBar::sub-line:vertical {
  subcontrol-position: top;
  background-color: #1d2127;
  margin: 0;
  height: 16;
}
QScrollBar::up-arrow:vertical {
  image: url('../Default/imgs/white/scrollbar/scroll-up.svg');
  image-position: center center;
}
QScrollBar::up-arrow:vertical:hover {
  image: url('../Default/imgs/white/scrollbar/scroll-up-hover.svg');
}
QScrollBar::up-arrow:vertical:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/white/scrollbar/scroll-up-hover.svg');
}
QScrollBar::down-arrow:vertical {
  image: url('../Default/imgs/white/scrollbar/scroll-down.svg');
  image-position: center center;
}
QScrollBar::down-arrow:vertical:hover {
  image: url('../Default/imgs/white/scrollbar/scroll-down-hover.svg');
}
QScrollBar::down-arrow:vertical:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/white/scrollbar/scroll-down-hover.svg');
}
QScrollBar::left-arrow:horizontal {
  image: url('../Default/imgs/white/scrollbar/scroll-left.svg');
  image-position: center center;
}
QScrollBar::left-arrow:horizontal:hover {
  image: url('../Default/imgs/white/scrollbar/scroll-left-hover.svg');
}
QScrollBar::left-arrow:horizontal:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/white/scrollbar/scroll-left-hover.svg');
}
QScrollBar::right-arrow:horizontal {
  image: url('../Default/imgs/white/scrollbar/scroll-right.svg');
  image-position: center center;
}
QScrollBar::right-arrow:horizontal:hover {
  image: url('../Default/imgs/white/scrollbar/scroll-right-hover.svg');
}
QScrollBar::right-arrow:horizontal:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/white/scrollbar/scroll-right.svg');
}
QScrollBar::sub-page:horizontal,
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:vertical,
QScrollBar::add-page:vertical {
  background: none;
}
/* -----------------------------------------------------------------------------
   Progress Bar
----------------------------------------------------------------------------- */
QProgressBar {
  text-align: center;
  background-color: #1a1d23;
  border: 1 solid #010102;
  border-radius: 3;
  padding: 0;
}
QProgressBar::chunk {
  margin: -1;
  background-color: #399739;
  border: 1 solid #010102;
  border-radius: 2;
}
/* -----------------------------------------------------------------------------
   Buttons
----------------------------------------------------------------------------- */
QPushButton {
  background-color: #414a57;
  border-color: #414a57;
  border-width: 1;
  border-style: solid;
  border-radius: 2px;
  color: #d3d3d3;
  margin: 0;
  padding: 2px 12px;
}
QPushButton:hover {
  background-color: #525e6f;
  border-color: #525e6f;
  color: #d3d3d3;
}
QPushButton:pressed {
  background-color: #1a1d23;
  border-color: #108183;
  color: #d3d3d3;
}
QPushButton:checked {
  background-color: #1a1d23;
  border-color: #108183;
  color: #75dedf;
}
QPushButton:checked:hover {
  background-color: #2b313a;
  border-color: #108183;
}
QPushButton:checked:pressed {
  background-color: #1a1d23;
}
QPushButton:disabled {
  background-color: #38404c;
  border-color: rgba(56, 64, 76, 0.4);
  color: rgba(211, 211, 211, 0.4);
}
#PushButton_NoPadding {
  padding: 3;
}
#CustomPanelButton {
  background-color: #414a57;
  border-color: #414a57;
  border-width: 1;
  border-style: solid;
  border-radius: 2px;
  color: #d3d3d3;
  margin: 0;
  padding: 2px 12px;
  padding: 0;
}
#CustomPanelButton:hover {
  background-color: #525e6f;
  border-color: #525e6f;
  color: #d3d3d3;
}
#CustomPanelButton:pressed {
  background-color: #1a1d23;
  border-color: #108183;
  color: #d3d3d3;
}
#CustomPanelButton:checked {
  background-color: #1a1d23;
  border-color: #108183;
  color: #75dedf;
}
#CustomPanelButton:checked:hover {
  background-color: #2b313a;
  border-color: #108183;
}
#CustomPanelButton:checked:pressed {
  background-color: #1a1d23;
}
#CustomPanelButton:disabled {
  background-color: #38404c;
  border-color: rgba(56, 64, 76, 0.4);
  color: rgba(211, 211, 211, 0.4);
}
/* -----------------------------------------------------------------------------
   ComboBox
----------------------------------------------------------------------------- */
QComboBox {
  background-color: #414a57;
  border-color: #414a57;
  border-width: 1;
  border-style: solid;
  border-radius: 2px;
  color: #d3d3d3;
  margin: 0;
  padding: 1 0 1 4;
}
QComboBox:hover {
  background-color: #525e6f;
  color: #d3d3d3;
  border-color: #525e6f;
}
QComboBox:checked {
  border-color: #aa4ca2;
}
QComboBox:pressed {
  background-color: #414a57;
  border-color: #aa4ca2;
  color: #d3d3d3;
}
QComboBox:pressed:focus {
  background-color: #414a57;
  border-color: #aa4ca2;
}
QComboBox:disabled {
  background-color: #38404c;
  border-color: rgba(56, 64, 76, 0.4);
  color: rgba(211, 211, 211, 0.4);
}
QComboBox:focus {
  background-color: #414a57;
  border-color: #aa4ca2;
}
QComboBox:focus:hover {
  background-color: #525e6f;
}
QComboBox:editable {
  color: #d3d3d3;
  background: none;
  background-color: #131519;
  border-color: #454f5d;
  selection-background-color: rgba(170, 76, 162, 0.8);
  selection-color: white;
  padding: 1 0 1 3;
}
QComboBox:editable:hover {
  background-color: #131519;
  border-color: #454f5d;
}
QComboBox:editable:checked {
  border-color: #aa4ca2;
}
QComboBox:editable:focus {
  border-color: #aa4ca2;
}
QComboBox::drop-down {
  border: 0 solid transparent;
  image: url('../Default/imgs/white/combobox/combo_downarrow.svg');
  image-position: center center;
  width: 16;
}
QComboBox::drop-down:disabled {
  image: url('../Default/imgs/white/combobox/combo_downarrow_disabled.svg');
}
QComboBox::drop-down:disabled:editable {
  background-color: #38404c;
}
QComboBox::drop-down:editable {
  background-color: #414a57;
  border-left: 1 solid #010102;
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
}
QComboBox::drop-down:editable:hover {
  background-color: #525e6f;
}
QComboBox QAbstractItemView {
  background-color: #2b313a;
  selection-background-color: #aa4ca2;
  selection-color: #ffffff;
  border: 1 solid #010102;
}
/* -----------------------------------------------------------------------------
   Textfields
----------------------------------------------------------------------------- */
QPlainTextEdit,
QLineEdit,
QTextEdit[readOnly="false"] {
  background-color: #131519;
  border-style: solid;
  border-width: 1;
  border-color: #454f5d;
  border-radius: 2px;
  color: #d3d3d3;
  selection-background-color: rgba(170, 76, 162, 0.8);
  selection-color: white;
  padding: 1 0 1 1;
}
QPlainTextEdit:hover,
QLineEdit:hover,
QTextEdit[readOnly="false"]:hover {
  background-color: #131519;
  color: #d3d3d3;
  border-color: #454f5d;
}
QPlainTextEdit:focus,
QLineEdit:focus,
QTextEdit[readOnly="false"]:focus {
  background-color: #131519;
  border-color: #aa4ca2;
  color: white;
}
QPlainTextEdit:disabled,
QLineEdit:disabled,
QTextEdit[readOnly="false"]:disabled {
  background: none;
  color: #686b6f;
  background-color: #21262d;
  border-color: rgba(69, 79, 93, 0.4);
}
QTextEdit[readOnly="true"] {
  background-color: #2b313a;
  color: #d3d3d3;
  border: 0;
}
QTextEdit[readOnly="true"]:focus,
QTextEdit[readOnly="true"]:disabled {
  background-color: #2b313a;
  color: #d3d3d3;
  border: 0;
}
/* -----------------------------------------------------------------------------
   Options
----------------------------------------------------------------------------- */
QCheckBox {
  color: #d3d3d3;
  padding: 0 2 0 0;
  border-radius: 2;
}
QCheckBox:hover {
  background-color: #3c4551;
  color: white;
}
QCheckBox:disabled {
  color: rgba(211, 211, 211, 0.4);
}
QCheckBox::indicator {
  background-color: #131519;
  border: 1 solid #010102;
  border-radius: 2px;
  padding: 1;
  width: 10px;
  height: 10px;
}
QCheckBox::indicator:hover {
  background-color: #242931;
  border-color: #242931;
}
QCheckBox::indicator:checked {
  background-color: #d8d80f;
  border-color: #d8d80f;
  image: url('../Default/imgs/white/checkmark/checkmark_black.svg');
}
QCheckBox::indicator:checked:hover {
  background-color: #f0f020;
  border-color: #f0f020;
}
QCheckBox::indicator:checked:disabled {
  background-color: #939520;
  border-color: #939520;
  image: url('../Default/imgs/white/checkmark/checkmark_disabled_black.svg');
}
QCheckBox::indicator:indeterminate {
  background-color: #d8d80f;
  border-color: #d8d80f;
  image: url('../Default/imgs/white/checkmark/checkpartially_black.svg');
}
QCheckBox::indicator:indeterminate:hover {
  background-color: #242931;
  border-color: #242931;
}
QCheckBox::indicator:indeterminate:disabled {
  background-color: #707429;
  border-color: #707429;
  image: url('../Default/imgs/white/checkmark/checkpartially_disabled_black.svg');
}
QCheckBox::indicator:disabled {
  background-color: #21262d;
  border-color: #1a1e24;
}
QRadioButton {
  color: #d3d3d3;
}
QRadioButton:hover {
  color: white;
}
QRadioButton:checked {
  color: #d3d3d3;
}
QRadioButton:disabled {
  color: rgba(211, 211, 211, 0.4);
}
QRadioButton::indicator {
  width: 13;
  height: 13;
  background-color: transparent;
  border: 0;
  image-position: center center;
}
QRadioButton::indicator:unchecked {
  image: url('../Default/imgs/white/radiobutton/radiobutton_unchecked.svg');
}
QRadioButton::indicator:unchecked:hover {
  image: url('../Default/imgs/white/radiobutton/radiobutton_unchecked_hover.svg');
}
QRadioButton::indicator:checked {
  image: url('../Default/imgs/white/radiobutton/radiobutton_checked.svg');
}
QRadioButton::indicator:checked:disabled {
  background-color: transparent;
  image: url('../Default/imgs/white/radiobutton/radiobutton_checked_disabled.svg');
}
QRadioButton::indicator:disabled {
  image: url('../Default/imgs/white/radiobutton/radiobutton_unchecked_disabled.svg');
}
/* -----------------------------------------------------------------------------
   GroupBox
----------------------------------------------------------------------------- */
QGroupBox {
  background-color: transparent;
  border-width: 1;
  border-style: solid;
  border-radius: 2px;
  border-color: #010102;
  margin: 6 0 0 0;
  padding: 5 5;
}
QGroupBox::title {
  subcontrol-origin: margin;
  color: #d3d3d3;
  left: 15;
  margin: -2 0 0 0;
  padding: 0 3;
  border-radius: 2;
}
QGroupBox::title:hover {
  color: white;
  background-color: #3c4551;
}
QGroupBox::title:disabled {
  color: rgba(211, 211, 211, 0.4);
}
QGroupBox::indicator {
  background-color: #131519;
  border: 1 solid #010102;
  border-radius: 2px;
  padding: 1;
  width: 10px;
  height: 10px;
  subcontrol-origin: margin;
  margin-top: 2;
}
QGroupBox::indicator:hover {
  background-color: #242931;
  border-color: #242931;
}
QGroupBox::indicator:checked {
  background-color: #d8d80f;
  border-color: #d8d80f;
  image: url('../Default/imgs/white/checkmark/checkmark_black.svg');
}
QGroupBox::indicator:checked:hover {
  background-color: #f0f020;
  border-color: #f0f020;
}
QGroupBox::indicator:checked:disabled {
  background-color: #939520;
  border-color: #939520;
  image: url('../Default/imgs/white/checkmark/checkmark_disabled_black.svg');
}
QGroupBox::indicator:indeterminate {
  background-color: #d8d80f;
  border-color: #d8d80f;
  image: url('../Default/imgs/white/checkmark/checkpartially_black.svg');
}
QGroupBox::indicator:indeterminate:hover {
  background-color: #242931;
  border-color: #242931;
}
QGroupBox::indicator:indeterminate:disabled {
  background-color: #707429;
  border-color: #707429;
  image: url('../Default/imgs/white/checkmark/checkpartially_disabled_black.svg');
}
