* {
  padding: 0;
  margin: 0;
  outline: 0;
  border: 0;
  border-style: none;
  border-image: none;
}
QToolBar * {
  padding: 0;
  margin: 0;
}
:TOONZCOLORS {
  -bg-color: #484848;
  -icon-base-opacity: 0.8;
  -icon-disabled-opacity: 0.3;
  -icon-base-color: #dfdfdf;
  -icon-active-color: white;
  -icon-selected-color: white;
  -icon-on-color: white;
  -icon-close-color: white;
  -icon-preview-color: black;
  -icon-vcheck-color: white;
  -icon-lock-color: white;
  -icon-keyframe-color: white;
  -icon-keyframe-modified-color: white;
}
QWidget {
  background-color: #484848;
  color: #dfdfdf;
}
QWidget:disabled {
  color: rgba(223, 223, 223, 0.4);
}
/* -----------------------------------------------------------------------------
   MainWindow & Floating Panels
----------------------------------------------------------------------------- */
QFrame {
  border: 0;
  margin: 0;
  padding: 0;
}
QToolTip,
#helpToolTip {
  background-color: #181818;
  border: 1 solid #828282;
  color: #d2d2d2;
  padding: 2px;
}
#DockSeparator,
QMainWindow::separator,
QSplitter::handle {
  background-color: #101010;
  height: 4;
  width: 4;
}
#TDockPlaceholder {
  background-color: #1aff00;
}
TPanel {
  background-color: #101010;
}
/* -----------------------------------------------------------------------------
   Topbar
----------------------------------------------------------------------------- */
#TopBar {
  background-color: #484848;
  border: 0;
  border-bottom: 1 solid #1b1b1b;
  margin: 0;
}
#TopBar #EditToolLockButton {
  background-color: #484848;
  spacing: 0;
}
#TopBar #EditToolLockButton::indicator {
  background: none;
  border: none;
  padding-left: 0;
  padding-right: 0;
}
#TopBarTabContainer {
  background-color: #484848;
  margin-bottom: 1;
}
#StackedMenuBar {
  border: 0;
  margin: 0;
  padding: 0;
}
QMenuBar {
  background-color: #484848;
  border: 0;
}
QMenuBar::item {
  background-color: #484848;
  border-left: 1 solid #484848;
  padding: 3 5;
}
QMenuBar::item:selected {
  background-color: #5c5c5c;
  color: white;
}
QMenuBar::item:pressed {
  background-color: #278d44;
  color: white;
}
/* -----------------------------------------------------------------------------
   Rooms Workspaces
----------------------------------------------------------------------------- */
#TopBarTab {
  margin: 0;
  padding: 0;
  border: 0;
  background-color: transparent;
}
#TopBarTab::tab {
  background-color: #3b3b3b;
  padding: 2 8 3 8;
  margin: 0 -1 0 0;
  border-color: #1b1b1b;
  border-radius: 0;
  border-bottom-width: 0;
}
#TopBarTab::tab:hover {
  background-color: #505050;
}
#TopBarTab::tab:selected {
  background-color: #278d44;
  border-color: #1b1b1b;
  color: white;
}
#TopBarTab::tab:selected:hover {
  background-color: #30ad53;
}
#TopBarTab::tab:first {
  border-left-width: 1;
  border-top-left-radius: 2;
}
#TopBarTab::tab:last {
  margin-right: 0;
  border-top-right-radius: 2;
}
#TopBarTab::tab:only-one {
  border-left-width: 1;
}
#TopBarTab QToolButton {
  border-left: 2 solid #1b1b1b;
  border-top: 1 solid #1b1b1b;
  border-right: 1 solid #1b1b1b;
  border-bottom: 0;
  margin-left: -1;
  margin-top: 0;
}
/* -----------------------------------------------------------------------------
   Context Menu
----------------------------------------------------------------------------- */
QMenu {
  background-color: #484848;
  border: 1 solid #1b1b1b;
  color: #dfdfdf;
  padding: 2 0;
}
QMenu::item {
  border: 0;
  padding: 3 28 3 14;
}
QMenu::item:selected {
  background-color: #278d44;
  color: white;
}
QMenu::item:checked {
  color: #66da87;
  background-color: #484848;
  font-weight: bold;
}
QMenu::item:checked:selected {
  background-color: #278d44;
  color: white;
}
QMenu::item:disabled {
  background: none;
  color: rgba(223, 223, 223, 0.4);
}
QMenu::item:disabled:selected {
  color: rgba(223, 223, 223, 0.4);
}
QMenu::separator {
  border-top: 1 solid #1b1b1b;
  height: 0;
  margin: 2 0;
}
QMenu::icon {
  border-radius: 2;
  position: absolute;
  left: 7;
  padding: 0;
  margin: 0;
  qproperty-icon: url('../Default/imgs/white/misc/transparent_16x16.png');
}
QMenu::icon:checked {
  background-color: #278d44;
  border: 1 solid #278d44;
}
QMenu::indicator {
  margin-left: 8;
}
QMenu::indicator:non-exclusive {
  background-color: #222222;
  border: 1 solid #1b1b1b;
  border-radius: 2px;
  padding: 1;
  width: 10px;
  height: 10px;
  border-color: #1b1b1b;
}
QMenu::indicator:non-exclusive:hover {
  background-color: #363636;
  border-color: #363636;
}
QMenu::indicator:non-exclusive:checked {
  background-color: #278d44;
  border-color: #278d44;
  image: url('../Default/imgs/white/checkmark/checkmark.svg');
}
QMenu::indicator:non-exclusive:checked:hover {
  background-color: #30ad53;
  border-color: #30ad53;
}
QMenu::indicator:non-exclusive:checked:disabled {
  background-color: #347146;
  border-color: #347146;
  image: url('../Default/imgs/white/checkmark/checkmark_disabled.svg');
}
QMenu::indicator:non-exclusive:indeterminate {
  background-color: #278d44;
  border-color: #278d44;
  image: url('../Default/imgs/white/checkmark/checkpartially.svg');
}
QMenu::indicator:non-exclusive:indeterminate:hover {
  background-color: #363636;
  border-color: #363636;
}
QMenu::indicator:non-exclusive:indeterminate:disabled {
  background-color: #3b6446;
  border-color: #3b6446;
  image: url('../Default/imgs/white/checkmark/checkpartially_disabled.svg');
}
QMenu::indicator:non-exclusive:disabled {
  background-color: #393939;
  border-color: #363636;
}
QMenu::indicator:non-exclusive:selected {
  border-color: #222222;
}
QMenu::indicator:non-exclusive:checked {
  border-color: #278d44;
}
QMenu::indicator:non-exclusive:checked:selected {
  border-color: #278d44;
}
QMenu::indicator:exclusive {
  width: 13;
  height: 13;
  background-color: transparent;
  border: 0;
  image-position: center center;
}
QMenu::indicator:exclusive:unchecked {
  image: url('../Default/imgs/white/radiobutton/radiobutton_unchecked.svg');
}
QMenu::indicator:exclusive:unchecked:hover {
  image: url('../Default/imgs/white/radiobutton/radiobutton_unchecked_hover.svg');
}
QMenu::indicator:exclusive:checked {
  image: url('../Default/imgs/white/radiobutton/radiobutton_checked.svg');
}
QMenu::indicator:exclusive:checked:disabled {
  background-color: transparent;
  image: url('../Default/imgs/white/radiobutton/radiobutton_checked_disabled.svg');
}
QMenu::indicator:exclusive:disabled {
  image: url('../Default/imgs/white/radiobutton/radiobutton_unchecked_disabled.svg');
}
/* -----------------------------------------------------------------------------
   Titlebars
----------------------------------------------------------------------------- */
TPanelTitleBar {
  background-color: #3b3b3b;
  border-bottom: 1 solid #1b1b1b;
  border-top-left-radius: 0px;
  border-top-right-radius: 0px;
  height: 18;
  min-height: 18;
  qproperty-TitleColor: rgba(223, 223, 223, 0.6);
  qproperty-ActiveTitleColor: #efefef;
  qproperty-CloseOverColor: #d44a4a;
  font-weight: bold;
}
TPanelTitleBar:hover {
  background-color: #3b3b3b;
}
TPanelTitleBarButton {
  qproperty-OffColor: #3b3b3b;
  qproperty-OverColor: #626262;
  qproperty-PressedColor: #278d44;
  qproperty-FreezeColor: #d44a4a;
  qproperty-PreviewColor: #C8C864;
}
/* -----------------------------------------------------------------------------
   Scrollbars
----------------------------------------------------------------------------- */
QAbstractScrollArea::corner {
  background-color: #343434;
}
QScrollBar {
  background-color: #343434;
  border: 0;
}
QScrollBar:horizontal {
  height: 15;
  margin: 0;
}
QScrollBar:vertical {
  margin: 0;
  width: 15;
}
QScrollBar::handle {
  background-color: #626262;
  border-style: solid;
  border-width: 1;
  border-color: #626262;
  border-radius: 5px;
}
QScrollBar::handle:horizontal:hover,
QScrollBar::handle:vertical:hover {
  background-color: #767676;
  border-color: #767676;
}
QScrollBar::handle:horizontal:pressed,
QScrollBar::handle:vertical:pressed {
  background-color: #8a8a8a;
  border-color: #8a8a8a;
}
QScrollBar::handle:horizontal {
  margin: 2px 16;
  min-width: 20;
}
QScrollBar::handle:vertical {
  margin: 16 2px;
  min-height: 20;
}
QScrollBar::add-line {
  subcontrol-origin: margin;
  border: 0;
}
QScrollBar::add-line:horizontal {
  subcontrol-position: right;
  background-color: #343434;
  margin: 0;
  width: 16;
}
QScrollBar::add-line:vertical {
  subcontrol-position: bottom;
  background-color: #343434;
  margin: 0;
  height: 16;
}
QScrollBar::sub-line {
  border: 0;
  subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
  subcontrol-position: left;
  background-color: #343434;
  margin: 0;
  width: 16;
}
QScrollBar::sub-line:vertical {
  subcontrol-position: top;
  background-color: #343434;
  margin: 0;
  height: 16;
}
QScrollBar::up-arrow:vertical {
  image: url('../Default/imgs/white/scrollbar/scroll-up.svg');
  image-position: center center;
}
QScrollBar::up-arrow:vertical:hover {
  image: url('../Default/imgs/white/scrollbar/scroll-up-hover.svg');
}
QScrollBar::up-arrow:vertical:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/white/scrollbar/scroll-up-hover.svg');
}
QScrollBar::down-arrow:vertical {
  image: url('../Default/imgs/white/scrollbar/scroll-down.svg');
  image-position: center center;
}
QScrollBar::down-arrow:vertical:hover {
  image: url('../Default/imgs/white/scrollbar/scroll-down-hover.svg');
}
QScrollBar::down-arrow:vertical:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/white/scrollbar/scroll-down-hover.svg');
}
QScrollBar::left-arrow:horizontal {
  image: url('../Default/imgs/white/scrollbar/scroll-left.svg');
  image-position: center center;
}
QScrollBar::left-arrow:horizontal:hover {
  image: url('../Default/imgs/white/scrollbar/scroll-left-hover.svg');
}
QScrollBar::left-arrow:horizontal:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/white/scrollbar/scroll-left-hover.svg');
}
QScrollBar::right-arrow:horizontal {
  image: url('../Default/imgs/white/scrollbar/scroll-right.svg');
  image-position: center center;
}
QScrollBar::right-arrow:horizontal:hover {
  image: url('../Default/imgs/white/scrollbar/scroll-right-hover.svg');
}
QScrollBar::right-arrow:horizontal:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/white/scrollbar/scroll-right.svg');
}
QScrollBar::sub-page:horizontal,
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:vertical,
QScrollBar::add-page:vertical {
  background: none;
}
/* -----------------------------------------------------------------------------
   Progress Bar
----------------------------------------------------------------------------- */
QProgressBar {
  text-align: center;
  background-color: #2f2f2f;
  border: 1 solid #1b1b1b;
  border-radius: 3;
  padding: 0;
}
QProgressBar::chunk {
  margin: -1;
  background-color: #399739;
  border: 1 solid #1b1b1b;
  border-radius: 2;
}
/* -----------------------------------------------------------------------------
   Buttons
----------------------------------------------------------------------------- */
QPushButton {
  background-color: #626262;
  border-color: #626262;
  border-width: 1;
  border-style: solid;
  border-radius: 2px;
  color: #dfdfdf;
  margin: 0;
  padding: 2px 12px;
}
QPushButton:hover {
  background-color: #767676;
  border-color: #767676;
  color: #dfdfdf;
}
QPushButton:pressed {
  background-color: #343434;
  border-color: #626262;
  color: #dfdfdf;
}
QPushButton:checked {
  background-color: #343434;
  border-color: #626262;
  color: #66da87;
}
QPushButton:checked:hover {
  background-color: #484848;
  border-color: #626262;
}
QPushButton:checked:pressed {
  background-color: #343434;
}
QPushButton:disabled {
  background-color: #575757;
  border-color: rgba(87, 87, 87, 0.4);
  color: rgba(223, 223, 223, 0.4);
}
#PushButton_NoPadding {
  padding: 3;
}
#CustomPanelButton {
  background-color: #626262;
  border-color: #626262;
  border-width: 1;
  border-style: solid;
  border-radius: 2px;
  color: #dfdfdf;
  margin: 0;
  padding: 2px 12px;
  padding: 0;
}
#CustomPanelButton:hover {
  background-color: #767676;
  border-color: #767676;
  color: #dfdfdf;
}
#CustomPanelButton:pressed {
  background-color: #343434;
  border-color: #626262;
  color: #dfdfdf;
}
#CustomPanelButton:checked {
  background-color: #343434;
  border-color: #626262;
  color: #66da87;
}
#CustomPanelButton:checked:hover {
  background-color: #484848;
  border-color: #626262;
}
#CustomPanelButton:checked:pressed {
  background-color: #343434;
}
#CustomPanelButton:disabled {
  background-color: #575757;
  border-color: rgba(87, 87, 87, 0.4);
  color: rgba(223, 223, 223, 0.4);
}
/* -----------------------------------------------------------------------------
   ComboBox
----------------------------------------------------------------------------- */
QComboBox {
  background-color: #626262;
  border-color: #626262;
  border-width: 1;
  border-style: solid;
  border-radius: 2px;
  color: #dfdfdf;
  margin: 0;
  padding: 1 0 1 4;
}
QComboBox:hover {
  background-color: #767676;
  color: #dfdfdf;
  border-color: #767676;
}
QComboBox:checked {
  border-color: #278d44;
}
QComboBox:pressed {
  background-color: #626262;
  border-color: #278d44;
  color: #dfdfdf;
}
QComboBox:pressed:focus {
  background-color: #626262;
  border-color: #278d44;
}
QComboBox:disabled {
  background-color: #575757;
  border-color: rgba(87, 87, 87, 0.4);
  color: rgba(223, 223, 223, 0.4);
}
QComboBox:focus {
  background-color: #626262;
  border-color: #278d44;
}
QComboBox:focus:hover {
  background-color: #767676;
}
QComboBox:editable {
  color: #dfdfdf;
  background: none;
  background-color: #222222;
  border-color: #222222;
  selection-background-color: rgba(39, 141, 68, 0.8);
  selection-color: white;
  padding: 1 0 1 3;
}
QComboBox:editable:hover {
  background-color: #222222;
  border-color: #222222;
}
QComboBox:editable:checked {
  border-color: #278d44;
}
QComboBox:editable:focus {
  border-color: #278d44;
}
QComboBox::drop-down {
  border: 0 solid transparent;
  image: url('../Default/imgs/white/combobox/combo_downarrow.svg');
  image-position: center center;
  width: 16;
}
QComboBox::drop-down:disabled {
  image: url('../Default/imgs/white/combobox/combo_downarrow_disabled.svg');
}
QComboBox::drop-down:disabled:editable {
  background-color: #575757;
}
QComboBox::drop-down:editable {
  background-color: #626262;
  border-left: 1 solid #1b1b1b;
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
}
QComboBox::drop-down:editable:hover {
  background-color: #767676;
}
QComboBox QAbstractItemView {
  background-color: #484848;
  selection-background-color: #278d44;
  selection-color: white;
  border: 1 solid #1b1b1b;
}
/* -----------------------------------------------------------------------------
   Textfields
----------------------------------------------------------------------------- */
QPlainTextEdit,
QLineEdit,
QTextEdit[readOnly="false"] {
  background-color: #222222;
  border-style: solid;
  border-width: 1;
  border-color: #222222;
  border-radius: 2px;
  color: #dfdfdf;
  selection-background-color: rgba(39, 141, 68, 0.8);
  selection-color: white;
  padding: 1 0 1 1;
}
QPlainTextEdit:hover,
QLineEdit:hover,
QTextEdit[readOnly="false"]:hover {
  background-color: #222222;
  color: #dfdfdf;
  border-color: #222222;
}
QPlainTextEdit:focus,
QLineEdit:focus,
QTextEdit[readOnly="false"]:focus {
  background-color: #222222;
  border-color: #278d44;
  color: white;
}
QPlainTextEdit:disabled,
QLineEdit:disabled,
QTextEdit[readOnly="false"]:disabled {
  background: none;
  color: #7b7b7b;
  background-color: #393939;
  border-color: rgba(34, 34, 34, 0.4);
}
QTextEdit[readOnly="true"] {
  background-color: #484848;
  color: #dfdfdf;
  border: 0;
}
QTextEdit[readOnly="true"]:focus,
QTextEdit[readOnly="true"]:disabled {
  background-color: #484848;
  color: #dfdfdf;
  border: 0;
}
/* -----------------------------------------------------------------------------
   Options
----------------------------------------------------------------------------- */
QCheckBox {
  color: #dfdfdf;
  padding: 0 2 0 0;
  border-radius: 2;
}
QCheckBox:hover {
  background-color: #5c5c5c;
  color: white;
}
QCheckBox:disabled {
  color: rgba(223, 223, 223, 0.4);
}
QCheckBox::indicator {
  background-color: #222222;
  border: 1 solid #1b1b1b;
  border-radius: 2px;
  padding: 1;
  width: 10px;
  height: 10px;
}
QCheckBox::indicator:hover {
  background-color: #363636;
  border-color: #363636;
}
QCheckBox::indicator:checked {
  background-color: #278d44;
  border-color: #278d44;
  image: url('../Default/imgs/white/checkmark/checkmark.svg');
}
QCheckBox::indicator:checked:hover {
  background-color: #30ad53;
  border-color: #30ad53;
}
QCheckBox::indicator:checked:disabled {
  background-color: #347146;
  border-color: #347146;
  image: url('../Default/imgs/white/checkmark/checkmark_disabled.svg');
}
QCheckBox::indicator:indeterminate {
  background-color: #278d44;
  border-color: #278d44;
  image: url('../Default/imgs/white/checkmark/checkpartially.svg');
}
QCheckBox::indicator:indeterminate:hover {
  background-color: #363636;
  border-color: #363636;
}
QCheckBox::indicator:indeterminate:disabled {
  background-color: #3b6446;
  border-color: #3b6446;
  image: url('../Default/imgs/white/checkmark/checkpartially_disabled.svg');
}
QCheckBox::indicator:disabled {
  background-color: #393939;
  border-color: #363636;
}
QRadioButton {
  color: #dfdfdf;
}
QRadioButton:hover {
  color: white;
}
QRadioButton:checked {
  color: #dfdfdf;
}
QRadioButton:disabled {
  color: rgba(223, 223, 223, 0.4);
}
QRadioButton::indicator {
  width: 13;
  height: 13;
  background-color: transparent;
  border: 0;
  image-position: center center;
}
QRadioButton::indicator:unchecked {
  image: url('../Default/imgs/white/radiobutton/radiobutton_unchecked.svg');
}
QRadioButton::indicator:unchecked:hover {
  image: url('../Default/imgs/white/radiobutton/radiobutton_unchecked_hover.svg');
}
QRadioButton::indicator:checked {
  image: url('../Default/imgs/white/radiobutton/radiobutton_checked.svg');
}
QRadioButton::indicator:checked:disabled {
  background-color: transparent;
  image: url('../Default/imgs/white/radiobutton/radiobutton_checked_disabled.svg');
}
QRadioButton::indicator:disabled {
  image: url('../Default/imgs/white/radiobutton/radiobutton_unchecked_disabled.svg');
}
/* -----------------------------------------------------------------------------
   GroupBox
----------------------------------------------------------------------------- */
QGroupBox {
  background-color: transparent;
  border-width: 1;
  border-style: solid;
  border-radius: 2px;
  border-color: #1b1b1b;
  margin: 6 0 0 0;
  padding: 5 5;
}
QGroupBox::title {
  subcontrol-origin: margin;
  color: #dfdfdf;
  left: 15;
  margin: -2 0 0 0;
  padding: 0 3;
  border-radius: 2;
}
QGroupBox::title:hover {
  color: white;
  background-color: #5c5c5c;
}
QGroupBox::title:disabled {
  color: rgba(223, 223, 223, 0.4);
}
QGroupBox::indicator {
  background-color: #222222;
  border: 1 solid #1b1b1b;
  border-radius: 2px;
  padding: 1;
  width: 10px;
  height: 10px;
  subcontrol-origin: margin;
  margin-top: 2;
}
QGroupBox::indicator:hover {
  background-color: #363636;
  border-color: #363636;
}
QGroupBox::indicator:checked {
  background-color: #278d44;
  border-color: #278d44;
  image: url('../Default/imgs/white/checkmark/checkmark.svg');
}
QGroupBox::indicator:checked:hover {
  background-color: #30ad53;
  border-color: #30ad53;
}
QGroupBox::indicator:checked:disabled {
  background-color: #347146;
  border-color: #347146;
  image: url('../Default/imgs/white/checkmark/checkmark_disabled.svg');
}
QGroupBox::indicator:indeterminate {
  background-color: #278d44;
  border-color: #278d44;
  image: url('../Default/imgs/white/checkmark/checkpartially.svg');
}
QGroupBox::indicator:indeterminate:hover {
  background-color: #363636;
  border-color: #363636;
}
QGroupBox::indicator:indeterminate:disabled {
  background-color: #3b6446;
  border-color: #3b6446;
  image: url('../Default/imgs/white/checkmark/checkpartially_disabled.svg');
}
QGroupBox::indicator:disabled {
  background-color: #393939;
  border-color: #363636;
}
QGroupBox:disabled {
  color: rgba(223, 223, 223, 0.4);
}
/* -----------------------------------------------------------------------------
   Sliders
----------------------------------------------------------------------------- */
QSlider {
  background-color: transparent;
}
QSlider::sub-page {
  background-color: #151515;
  border: 1 solid #151515;
  border-radius: 2;
}
