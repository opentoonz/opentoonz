* {
  padding: 0;
  margin: 0;
  outline: 0;
  border: 0;
  border-style: none;
  border-image: none;
}
QToolBar * {
  padding: 0;
  margin: 0;
}
:TOONZCOLORS {
  -bg-color: #DBDBDB;
  -icon-base-opacity: 0.8;
  -icon-disabled-opacity: 0.3;
  -icon-base-color: black;
  -icon-active-color: black;
  -icon-selected-color: black;
  -icon-on-color: black;
  -icon-close-color: black;
  -icon-preview-color: black;
  -icon-vcheck-color: black;
  -icon-lock-color: black;
  -icon-keyframe-color: black;
  -icon-keyframe-modified-color: black;
}
QWidget {
  background-color: #DBDBDB;
  color: black;
}
QWidget:disabled {
  color: rgba(0, 0, 0, 0.4);
}
/* -----------------------------------------------------------------------------
   MainWindow & Floating Panels
----------------------------------------------------------------------------- */
QFrame {
  border: 0;
  margin: 0;
  padding: 0;
}
QToolTip,
#helpToolTip {
  background-color: #181818;
  border: 1 solid #828282;
  color: #d2d2d2;
  padding: 2px;
}
#DockSeparator,
QMainWindow::separator,
QSplitter::handle {
  background-color: #4f4f4f;
  height: 4;
  width: 4;
}
#TDockPlaceholder {
  background-color: #1aff00;
}
TPanel {
  background-color: #4f4f4f;
}
/* -----------------------------------------------------------------------------
   Topbar
----------------------------------------------------------------------------- */
#TopBar {
  background-color: #DBDBDB;
  border: 0;
  border-bottom: 1 solid #929292;
  margin: 0;
}
#TopBar #EditToolLockButton {
  background-color: #DBDBDB;
  spacing: 0;
}
#TopBar #EditToolLockButton::indicator {
  background: none;
  border: none;
  padding-left: 0;
  padding-right: 0;
}
#TopBarTabContainer {
  background-color: #DBDBDB;
  margin-bottom: 1;
}
#StackedMenuBar {
  border: 0;
  margin: 0;
  padding: 0;
}
QMenuBar {
  background-color: #DBDBDB;
  border: 0;
}
QMenuBar::item {
  background-color: #DBDBDB;
  border-left: 1 solid #DBDBDB;
  padding: 3 5;
}
QMenuBar::item:selected {
  background-color: #bebebe;
  color: black;
}
QMenuBar::item:pressed {
  background-color: #A0C1DD;
  color: black;
}
/* -----------------------------------------------------------------------------
   Rooms Workspaces
----------------------------------------------------------------------------- */
#TopBarTab {
  margin: 0;
  padding: 0;
  border: 0;
  background-color: transparent;
}
#TopBarTab::tab {
  background-color: #c2c2c2;
  padding: 2 8 3 8;
  margin: 0 -1 0 0;
  border-color: #929292;
  border-radius: 0;
  border-bottom-width: 0;
}
#TopBarTab::tab:hover {
  background-color: #a5a5a5;
}
#TopBarTab::tab:selected {
  background-color: #A0C1DD;
  border-color: #929292;
  color: black;
}
#TopBarTab::tab:selected:hover {
  background-color: #76a6ce;
}
#TopBarTab::tab:first {
  border-left-width: 1;
  border-top-left-radius: 2;
}
#TopBarTab::tab:last {
  margin-right: 0;
  border-top-right-radius: 2;
}
#TopBarTab::tab:only-one {
  border-left-width: 1;
}
#TopBarTab QToolButton {
  border-left: 2 solid #929292;
  border-top: 1 solid #929292;
  border-right: 1 solid #929292;
  border-bottom: 0;
  margin-left: -1;
  margin-top: 0;
}
/* -----------------------------------------------------------------------------
   Context Menu
----------------------------------------------------------------------------- */
QMenu {
  background-color: #DBDBDB;
  border: 1 solid #929292;
  color: black;
  padding: 2 0;
}
QMenu::item {
  border: 0;
  padding: 3 28 3 14;
}
QMenu::item:selected {
  background-color: #A0C1DD;
  color: black;
}
QMenu::item:checked {
  color: #341f88;
  background-color: #DBDBDB;
  font-weight: bold;
}
QMenu::item:checked:selected {
  background-color: #A0C1DD;
  color: black;
}
QMenu::item:disabled {
  background: none;
  color: rgba(0, 0, 0, 0.4);
}
QMenu::item:disabled:selected {
  color: rgba(0, 0, 0, 0.4);
}
QMenu::separator {
  border-top: 1 solid #929292;
  height: 0;
  margin: 2 0;
}
QMenu::icon {
  border-radius: 2;
  position: absolute;
  left: 7;
  padding: 0;
  margin: 0;
  qproperty-icon: url('../Default/imgs/black/misc/transparent_16x16.png');
}
QMenu::icon:checked {
  background-color: lime;
  border: 1 solid lime;
}
QMenu::indicator {
  margin-left: 8;
}
QMenu::indicator:non-exclusive {
  background-color: #ffffff;
  border: 1 solid #929292;
  border-radius: 2px;
  padding: 1;
  width: 10px;
  height: 10px;
  border-color: #929292;
}
QMenu::indicator:non-exclusive:hover {
  background-color: #e2e2e2;
  border-color: #e2e2e2;
}
QMenu::indicator:non-exclusive:checked {
  background-color: black;
  border-color: black;
  image: url('../Default/imgs/black/checkmark/checkmark.svg');
}
QMenu::indicator:non-exclusive:checked:hover {
  background-color: #4e4e4e;
  border-color: black;
}
QMenu::indicator:non-exclusive:checked:disabled {
  background-color: #585858;
  border-color: #585858;
  image: url('../Default/imgs/black/checkmark/checkmark_disabled.svg');
}
QMenu::indicator:non-exclusive:indeterminate {
  background-color: black;
  border-color: black;
  image: url('../Default/imgs/black/checkmark/checkpartially.svg');
}
QMenu::indicator:non-exclusive:indeterminate:hover {
  background-color: #e2e2e2;
  border-color: #e2e2e2;
}
QMenu::indicator:non-exclusive:indeterminate:disabled {
  background-color: #838383;
  border-color: #838383;
  image: url('../Default/imgs/black/checkmark/checkpartially_disabled.svg');
}
QMenu::indicator:non-exclusive:disabled {
  background-color: #e9e9e9;
  border-color: #bebebe;
}
QMenu::indicator:non-exclusive:selected {
  border-color: #929292;
}
QMenu::indicator:non-exclusive:checked {
  border-color: black;
}
QMenu::indicator:non-exclusive:checked:selected {
  border-color: black;
}
QMenu::indicator:exclusive {
  width: 13;
  height: 13;
  background-color: transparent;
  border: 0;
  image-position: center center;
}
QMenu::indicator:exclusive:unchecked {
  image: url('../Default/imgs/black/radiobutton/radiobutton_unchecked.svg');
}
QMenu::indicator:exclusive:unchecked:hover {
  image: url('../Default/imgs/black/radiobutton/radiobutton_unchecked_hover.svg');
}
QMenu::indicator:exclusive:checked {
  image: url('../Default/imgs/black/radiobutton/radiobutton_checked.svg');
}
QMenu::indicator:exclusive:checked:disabled {
  background-color: transparent;
  image: url('../Default/imgs/black/radiobutton/radiobutton_checked_disabled.svg');
}
QMenu::indicator:exclusive:disabled {
  image: url('../Default/imgs/black/radiobutton/radiobutton_unchecked_disabled.svg');
}
/* -----------------------------------------------------------------------------
   Titlebars
----------------------------------------------------------------------------- */
TPanelTitleBar {
  background-color: #c2c2c2;
  border-bottom: 1 solid #929292;
  border-top-left-radius: 0;
  border-top-right-radius: 0;
  height: 18;
  min-height: 18;
  qproperty-TitleColor: rgba(0, 0, 0, 0.6);
  qproperty-ActiveTitleColor: black;
  qproperty-CloseOverColor: #f07676;
  font-weight: bold;
}
TPanelTitleBar:hover {
  background-color: #c2c2c2;
}
TPanelTitleBarButton {
  qproperty-OffColor: #c2c2c2;
  qproperty-OverColor: #8c8c8c;
  qproperty-PressedColor: white;
  qproperty-FreezeColor: #f07676;
  qproperty-PreviewColor: #FCFF53;
}
/* -----------------------------------------------------------------------------
   Scrollbars
----------------------------------------------------------------------------- */
QAbstractScrollArea::corner {
  background-color: #dbdbdb;
}
QScrollBar {
  background-color: #dbdbdb;
  border: 0;
}
QScrollBar:horizontal {
  height: 15;
  margin: 0;
}
QScrollBar:vertical {
  margin: 0;
  width: 15;
}
QScrollBar::handle {
  background-color: #9e9e9e;
  border-style: solid;
  border-width: 1;
  border-color: #9e9e9e;
  border-radius: 5px;
}
QScrollBar::handle:horizontal:hover,
QScrollBar::handle:vertical:hover {
  background-color: #818181;
  border-color: #818181;
}
QScrollBar::handle:horizontal:pressed,
QScrollBar::handle:vertical:pressed {
  background-color: #6e6e6e;
  border-color: #6e6e6e;
}
QScrollBar::handle:horizontal {
  margin: 2px 16;
  min-width: 20;
}
QScrollBar::handle:vertical {
  margin: 16 2px;
  min-height: 20;
}
QScrollBar::add-line {
  subcontrol-origin: margin;
  border: 0;
}
QScrollBar::add-line:horizontal {
  subcontrol-position: right;
  background-color: #dbdbdb;
  margin: 0;
  width: 16;
}
QScrollBar::add-line:vertical {
  subcontrol-position: bottom;
  background-color: #dbdbdb;
  margin: 0;
  height: 16;
}
QScrollBar::sub-line {
  border: 0;
  subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal {
  subcontrol-position: left;
  background-color: #dbdbdb;
  margin: 0;
  width: 16;
}
QScrollBar::sub-line:vertical {
  subcontrol-position: top;
  background-color: #dbdbdb;
  margin: 0;
  height: 16;
}
QScrollBar::up-arrow:vertical {
  image: url('../Default/imgs/black/scrollbar/scroll-up.svg');
  image-position: center center;
}
QScrollBar::up-arrow:vertical:hover {
  image: url('../Default/imgs/black/scrollbar/scroll-up-hover.svg');
}
QScrollBar::up-arrow:vertical:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/black/scrollbar/scroll-up-hover.svg');
}
QScrollBar::down-arrow:vertical {
  image: url('../Default/imgs/black/scrollbar/scroll-down.svg');
  image-position: center center;
}
QScrollBar::down-arrow:vertical:hover {
  image: url('../Default/imgs/black/scrollbar/scroll-down-hover.svg');
}
QScrollBar::down-arrow:vertical:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/black/scrollbar/scroll-down-hover.svg');
}
QScrollBar::left-arrow:horizontal {
  image: url('../Default/imgs/black/scrollbar/scroll-left.svg');
  image-position: center center;
}
QScrollBar::left-arrow:horizontal:hover {
  image: url('../Default/imgs/black/scrollbar/scroll-left-hover.svg');
}
QScrollBar::left-arrow:horizontal:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/black/scrollbar/scroll-left-hover.svg');
}
QScrollBar::right-arrow:horizontal {
  image: url('../Default/imgs/black/scrollbar/scroll-right.svg');
  image-position: center center;
}
QScrollBar::right-arrow:horizontal:hover {
  image: url('../Default/imgs/black/scrollbar/scroll-right-hover.svg');
}
QScrollBar::right-arrow:horizontal:pressed {
  margin: 1 0 0 0;
  image: url('../Default/imgs/black/scrollbar/scroll-right.svg');
}
QScrollBar::sub-page:horizontal,
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:vertical,
QScrollBar::add-page:vertical {
  background: none;
}
/* -----------------------------------------------------------------------------
   Progress Bar
----------------------------------------------------------------------------- */
QProgressBar {
  text-align: center;
  background-color: #c2c2c2;
  border: 1 solid #929292;
  border-radius: 3;
  padding: 0;
}
QProgressBar::chunk {
  margin: -1;
  background-color: #399739;
  border: 1 solid #929292;
  border-radius: 2;
}
/* -----------------------------------------------------------------------------
   Buttons
----------------------------------------------------------------------------- */
QPushButton {
  background-color: #cccccc;
  border-color: #8f8f8f;
  border-width: 1;
  border-style: solid;
  border-radius: 2px;
  color: black;
  margin: 0;
  padding: 2px 12px;
}
QPushButton:hover {
  background-color: #afafaf;
  border-color: #8f8f8f;
  color: black;
}
QPushButton:pressed {
  background-color: #8d8d8d;
  border-color: #707070;
  color: black;
}
QPushButton:checked {
  background-color: #A0C1DD;
  border-color: #707070;
  color: #341f88;
}
QPushButton:checked:hover {
  background-color: #76a6ce;
  border-color: #707070;
}
QPushButton:checked:pressed {
  background-color: #8d8d8d;
}
QPushButton:disabled {
  background-color: #d2d2d2;
  border-color: #bcbcbc;
  color: rgba(0, 0, 0, 0.4);
}
#PushButton_NoPadding {
  padding: 3;
}
#CustomPanelButton {
  background-color: #cccccc;
  border-color: #8f8f8f;
  border-width: 1;
  border-style: solid;
  border-radius: 2px;
  color: black;
  margin: 0;
  padding: 2px 12px;
  padding: 0;
}
#CustomPanelButton:hover {
  background-color: #afafaf;
  border-color: #8f8f8f;
  color: black;
}
#CustomPanelButton:pressed {
  background-color: #8d8d8d;
  border-color: #707070;
  color: black;
}
#CustomPanelButton:checked {
  background-color: #A0C1DD;
  border-color: #707070;
  color: #341f88;
}
#CustomPanelButton:checked:hover {
  background-color: #76a6ce;
  border-color: #707070;
}
#CustomPanelButton:checked:pressed {
  background-color: #8d8d8d;
}
#CustomPanelButton:disabled {
  background-color: #d2d2d2;
  border-color: #bcbcbc;
  color: rgba(0, 0, 0, 0.4);
}
/* -----------------------------------------------------------------------------
   ComboBox
----------------------------------------------------------------------------- */
QComboBox {
  background-color: #cccccc;
  border-color: #8f8f8f;
  border-width: 1;
  border-style: solid;
  border-radius: 2px;
  color: black;
  margin: 0;
  padding: 1 0 1 4;
}
QComboBox:hover {
  background-color: #afafaf;
  color: black;
  border-color: #8f8f8f;
}
QComboBox:checked {
  border-color: black;
}
QComboBox:pressed {
  background-color: #cccccc;
  border-color: black;
  color: black;
}
QComboBox:pressed:focus {
  background-color: #cccccc;
  border-color: black;
}
QComboBox:disabled {
  background-color: #d2d2d2;
  border-color: #bcbcbc;
  color: rgba(0, 0, 0, 0.4);
}
QComboBox:focus {
  background-color: #cccccc;
  border-color: black;
}
QComboBox:focus:hover {
  background-color: #afafaf;
}
QComboBox:editable {
  color: black;
  background: none;
  background-color: #ffffff;
  border-color: #929292;
  selection-background-color: rgba(160, 193, 221, 0.8);
  selection-color: black;
  padding: 1 0 1 3;
}
QComboBox:editable:hover {
  background-color: #ffffff;
  border-color: #929292;
}
QComboBox:editable:checked {
  border-color: black;
}
QComboBox:editable:focus {
  border-color: black;
}
QComboBox::drop-down {
  border: 0 solid transparent;
  image: url('../Default/imgs/black/combobox/combo_downarrow.svg');
  image-position: center center;
  width: 16;
}
QComboBox::drop-down:disabled {
  image: url('../Default/imgs/black/combobox/combo_downarrow_disabled.svg');
}
QComboBox::drop-down:disabled:editable {
  background-color: #d2d2d2;
}
QComboBox::drop-down:editable {
  background-color: #cccccc;
  border-left: 1 solid #929292;
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
}
QComboBox::drop-down:editable:hover {
  background-color: #afafaf;
}
QComboBox QAbstractItemView {
  background-color: #DBDBDB;
  selection-background-color: #A0C1DD;
  selection-color: black;
  border: 1 solid #929292;
}
/* -----------------------------------------------------------------------------
   Textfields
----------------------------------------------------------------------------- */
QPlainTextEdit,
QLineEdit,
QTextEdit[readOnly="false"] {
  background-color: #ffffff;
  border-style: solid;
  border-width: 1;
  border-color: #929292;
  border-radius: 2px;
  color: black;
  selection-background-color: rgba(160, 193, 221, 0.8);
  selection-color: black;
  padding: 1 0 1 1;
}
QPlainTextEdit:hover,
QLineEdit:hover,
QTextEdit[readOnly="false"]:hover {
  background-color: #ffffff;
  color: black;
  border-color: #929292;
}
QPlainTextEdit:focus,
QLineEdit:focus,
QTextEdit[readOnly="false"]:focus {
  background-color: #ffffff;
  border-color: black;
  color: black;
}
QPlainTextEdit:disabled,
QLineEdit:disabled,
QTextEdit[readOnly="false"]:disabled {
  background: none;
  color: #8c8c8c;
  background-color: #e9e9e9;
  border-color: rgba(146, 146, 146, 0.4);
}
QTextEdit[readOnly="true"] {
  background-color: #DBDBDB;
  color: black;
  border: 0;
}
QTextEdit[readOnly="true"]:focus,
QTextEdit[readOnly="true"]:disabled {
  background-color: #DBDBDB;
  color: black;
  border: 0;
}
/* -----------------------------------------------------------------------------
   Options
----------------------------------------------------------------------------- */
QCheckBox {
  color: black;
  padding: 0 2 0 0;
  border-radius: 2;
}
QCheckBox:hover {
  background-color: #bebebe;
  color: black;
}
QCheckBox:disabled {
  color: rgba(0, 0, 0, 0.4);
}
QCheckBox::indicator {
  background-color: #ffffff;
  border: 1 solid #929292;
  border-radius: 2px;
  padding: 1;
  width: 10px;
  height: 10px;
}
QCheckBox::indicator:hover {
  background-color: #e2e2e2;
  border-color: #e2e2e2;
}
QCheckBox::indicator:checked {
  background-color: black;
  border-color: black;
  image: url('../Default/imgs/black/checkmark/checkmark.svg');
}
QCheckBox::indicator:checked:hover {
  background-color: #4e4e4e;
  border-color: black;
}
QCheckBox::indicator:checked:disabled {
  background-color: #585858;
  border-color: #585858;
  image: url('../Default/imgs/black/checkmark/checkmark_disabled.svg');
}
QCheckBox::indicator:indeterminate {
  background-color: black;
  border-color: black;
  image: url('../Default/imgs/black/checkmark/checkpartially.svg');
}
QCheckBox::indicator:indeterminate:hover {
  background-color: #e2e2e2;
  border-color: #e2e2e2;
}
QCheckBox::indicator:indeterminate:disabled {
  background-color: #838383;
  border-color: #838383;
  image: url('../Default/imgs/black/checkmark/checkpartially_disabled.svg');
}
QCheckBox::indicator:disabled {
  background-color: #e9e9e9;
  border-color: #bebebe;
}
QRadioButton {
  color: black;
}
QRadioButton:hover {
  color: black;
}
QRadioButton:checked {
  color: black;
}
QRadioButton:disabled {
  color: rgba(0, 0, 0, 0.4);
}
QRadioButton::indicator {
  width: 13;
  height: 13;
  background-color: transparent;
  border: 0;
  image-position: center center;
}
QRadioButton::indicator:unchecked {
  image: url('../Default/imgs/black/radiobutton/radiobutton_unchecked.svg');
}
QRadioButton::indicator:unchecked:hover {
  image: url('../Default/imgs/black/radiobutton/radiobutton_unchecked_hover.svg');
}
QRadioButton::indicator:checked {
  image: url('../Default/imgs/black/radiobutton/radiobutton_checked.svg');
}
QRadioButton::indicator:checked:disabled {
  background-color: transparent;
  image: url('../Default/imgs/black/radiobutton/radiobutton_checked_disabled.svg');
}
QRadioButton::indicator:disabled {
  image: url('../Default/imgs/black/radiobutton/radiobutton_unchecked_disabled.svg');
}
/* -----------------------------------------------------------------------------
   GroupBox
----------------------------------------------------------------------------- */
QGroupBox {
  background-color: transparent;
  border-width: 1;
  border-style: solid;
  border-radius: 2px;
  border-color: #929292;
  margin: 6 0 0 0;
  padding: 5 5;
}
QGroupBox::title {
  subcontrol-origin: margin;
  color: black;
  left: 15;
  margin: -2 0 0 0;
  padding: 0 3;
  border-radius: 2;
}
QGroupBox::title:hover {
  color: black;
  background-color: #bebebe;
}
QGroupBox::title:disabled {
  color: rgba(0, 0, 0, 0.4);
}
QGroupBox::indicator {
  background-color: #ffffff;
  border: 1 solid #929292;
  border-radius: 2px;
  padding: 1;
  width: 10px;
  height: 10px;
  subcontrol-origin: margin;
  margin-top: 2;
}
QGroupBox::indicator:hover {
  background-color: #e2e2e2;
  border-color: #e2e2e2;
}
QGroupBox::indicator:checked {
  background-color: black;
  border-color: black;
  image: url('../Default/imgs/black/checkmark/checkmark.svg');
}
QGroupBox::indicator:checked:hover {
  background-color: #4e4e4e;
  border-color: black;
}
QGroupBox::indicator:checked:disabled {
  background-color: #585858;
  border-color: #585858;
  image: url('../Default/imgs/black/checkmark/checkmark_disabled.svg');
}
QGroupBox::indicator:indeterminate {
  background-color: black;
  border-color: black;
  image: url('../Default/imgs/black/checkmark/checkpartially.svg');
}
QGroupBox::indicator:indeterminate:hover {
  background-color: #e2e2e2;
  border-color: #e2e2e2;
}
QGroupBox::indicator:indeterminate:disabled {
  background-color: #838383;
  border-color: #838383;
  image: url('../Default/imgs/black/checkmark/checkpartially_disabled.svg');
}
QGroupBox::indicator:disabled {
  background-color: #e9e9e9;
  border-color: #bebebe;
}
QGroupBox:disabled {
  color: rgba(0, 0, 0, 0.4);
}
/* -----------------------------------------------------------------------------
   Sliders
----------------------------------------------------------------------------- */
QSlider {
  background-color: transparent;
}
QSlider::sub-page {
  background-color: #757575;
  border: 1 solid #757575;
  border-radius: 2;
}
QSlider::sub-page:disabled {
  background-color: #b2b2b2;
  border-color: #b2b2b2;
}
QSlider::sub-page:horizontal {
