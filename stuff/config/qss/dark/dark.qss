/* ------ Qt Widgets Common Difinitions ------ */
QWidget {
  color: #c8c8c8;
  background-color: #1e1e1e;
  font-family: "Segoe UI";
}
QFrame {
  margin: 0px;
  border: 0px;
  padding: 0px;
}
QDialog {
  background-color: #1e1e1e;
}
QMainWindow::separator {
  background: yellow;
  width: 10px;
  /* when vertical */
  height: 10px;
  /* when horizontal */
}
QToolTip,
#helpTooltip {
  border: 1px solid black;
  background-color: #ffffe1;
  padding: 2px;
  color: black;
}
QTreeWidget {
  border-width: 1px;
  background-color: #1e1e1e;
  border-style: inset;
  border-left-color: #0c0c0c;
  border-top-color: #000000;
  border-right-color: #444444;
  border-bottom-color: #585858;
  alternate-background-color: #2b2b2b;
}
QTreeWidget::item:selected {
  background-color: #217fbc;
  color: black;
}
QTreeWidget::item {
  color: #c8c8c8;
}
QStatusBar {
  background-color: #c0c0c0;
}
QStatusBar::item {
  border-width: 0;
}
QStatusBar QLabel {
  background-color: #c0c0c0;
}
QStatusBar #StatusBarLabel {
  background-color: #ffffff;
  padding-left: 3px;
  padding-right: 3px;
  padding-top: 1px;
  padding-bottom: 1px;
}
QMenuBar {
  background-color: #1e1e1e;
}
QMenuBar::item:selected {
  background-color: #040404;
  border-style: inset;
  border-left-color: #000000;
  border-top-color: #000000;
  border-right-color: #2b2b2b;
  border-bottom-color: #3f3f3f;
  border-width: 0px;
}
QMenu {
  background-color: #161616;
}
QMenu::item:selected {
  background: #217fbc;
}
QMenu::item:disabled {
  color: #808080;
}
QMenu::item:disabled:selected {
  background: #6c7680;
}
QMenu::separator {
  background-color: #000000;
  border-style: inset;
  border-left-color: #000000;
  border-top-color: #000000;
  border-right-color: #1e1e1e;
  border-bottom-color: #323232;
  margin-left: 5px;
  margin-right: 5px;
  margin-top: 2px;
  margin-bottom: 2px;
  border-width: 0px;
  border-bottom-width: 1px;
  height: 0px;
}
QToolBar {
  background-color: #1e1e1e;
  border-style: outset;
  border-left-color: #444444;
  border-top-color: #585858;
  border-right-color: #0c0c0c;
  border-bottom-color: #000000;
  border-width: 0px;
  margin: 0px;
  padding: 0px;
  border-image: none;
}
QToolBar::separator:horizontal {
  image: url("imgs/bottomseparator.png");
}
QToolBar::separator:vertical {
  image: url("imgs/separator.png");
}
QToolBar QToolButton {
  background-color: #1e1e1e;
  /*margin: 2px 1px 1px 1px;*/
  margin: 3px;
  border: 0px;
  border-image: none;
  border-radius: 2px;
}
QToolBar QToolButton:hover {
  border-image: url("imgs/over.png") 2;
}
QToolBar QToolButton:checked,
QToolBar QToolButton:pressed {
  border-image: url("imgs/click.png") 2;
}
QToolBar QToolButton:disabled {
  background-color: #2b2b2b;
  color: #808080;
}
QToolBar QToolButton::menu-indicator {
  image: none;
}
QToolBar QToolButton::menu-button {
  border-image: none;
  /*background-color: rgb(160,160,160);*/
}
QToolBar QLabel {
  background-color: #1e1e1e;
  margin-top: 1px;
  border-width: 2;
}
QToolBar QToolBar {
  border-width: 0px;
}
QLineEdit {
  /*darken little bit more*/
  background-color: #2b2b2b;
  border-style: inset;
  border-left-color: #191919;
  border-top-color: #0d0d0d;
