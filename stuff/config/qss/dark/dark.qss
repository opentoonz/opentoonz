  margin-top: 5px;
  margin-bottom: 5px;
  padding-left: 3px;
  padding-right: 3px;
  padding-top: 5px;
  padding-bottom: 5px;
}
QGroupBox::title {
  subcontrol-origin: margin;
  padding: 0px;
  margin-top: -4px;
  /*bottom: 3px;*/
  left: 15px;
}
QSplitter::handle {
