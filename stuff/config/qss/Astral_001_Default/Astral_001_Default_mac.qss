/* -----------------------------------------------------------------------------
   Component: Button Styles
----------------------------------------------------------------------------- */
.button-show,
#LoadLevelShowButton,
#CleanupSettingsShowButton,
#OutputSettingsShowButton,
#FxSettingsPreviewShowButton {
  image: url('imgs/white/plus.png');
  image-position: center center;
  margin: 0;
  padding: 0;
}
.button-show:checked,
#LoadLevelShowButton:checked,
#CleanupSettingsShowButton:checked,
#OutputSettingsShowButton:checked,
#FxSettingsPreviewShowButton:checked {
  background-color: #5c5c5c;
  border-color: #343434;
  image: url('imgs/white/minus.png');
}
.button-show:checked:pressed,
#LoadLevelShowButton:checked:pressed,
#CleanupSettingsShowButton:checked:pressed,
#OutputSettingsShowButton:checked:pressed,
#FxSettingsPreviewShowButton:checked:pressed {
  background-color: #343434;
  border-color: #1a1a1a;
}
.button-show:checked:hover,
#LoadLevelShowButton:checked:hover,
#CleanupSettingsShowButton:checked:hover,
#OutputSettingsShowButton:checked:hover,
#FxSettingsPreviewShowButton:checked:hover {
  background-color: #696969;
}
.button-tool,
QToolBar QToolButton,
#CameraSettingsRadioButton::indicator,
#ForceSquaredPixelButton,
#EditToolLockButton::indicator {
  background-color: #484848;
  border: 1px solid transparent;
  border-radius: 2px;
  margin: 1px;
  padding: 0;
}
.button-tool:hover,
QToolBar QToolButton:hover,
#CameraSettingsRadioButton::indicator:hover,
#ForceSquaredPixelButton:hover,
#EditToolLockButton::indicator:hover {
  background-color: #5c5c5c;
  border-color: #343434;
}
.button-tool:pressed,
QToolBar QToolButton:pressed,
#CameraSettingsRadioButton::indicator:pressed,
#ForceSquaredPixelButton:pressed,
#EditToolLockButton::indicator:pressed {
  background-color: #343434;
  border-color: #1a1a1a;
}
.button-tool:checked,
QToolBar QToolButton:checked,
#CameraSettingsRadioButton::indicator:checked,
#ForceSquaredPixelButton:checked,
#EditToolLockButton::indicator:checked {
  background-color: #272727;
  border-color: #0d0d0d;
}
.button-tool:disabled,
QToolBar QToolButton:disabled,
#CameraSettingsRadioButton::indicator:disabled,
#ForceSquaredPixelButton:disabled,
#EditToolLockButton::indicator:disabled {
  color: #737373;
}
.button-tool::menu-indicator,
QToolBar QToolButton::menu-indicator,
#CameraSettingsRadioButton::indicator::menu-indicator,
#ForceSquaredPixelButton::menu-indicator,
#EditToolLockButton::indicator::menu-indicator {
  image: none;
}
.button-tool::menu-button,
QToolBar QToolButton::menu-button,
#CameraSettingsRadioButton::indicator::menu-button,
#ForceSquaredPixelButton::menu-button,
#EditToolLockButton::indicator::menu-button {
  border-image: none;
}
.button-tool QLabel,
QToolBar QToolButton QLabel,
#CameraSettingsRadioButton::indicator QLabel,
#ForceSquaredPixelButton QLabel,
#EditToolLockButton::indicator QLabel {
  margin-top: 1px;
}
.button-tool QToolBar,
QToolBar QToolButton QToolBar,
#CameraSettingsRadioButton::indicator QToolBar,
#ForceSquaredPixelButton QToolBar,
#EditToolLockButton::indicator QToolBar {
  border: 0;
}
.button-flat,
PaletteViewer QToolBar QToolButton {
  background-color: none;
  border: 0;
  border-radius: 0;
}
.button-flat:hover,
PaletteViewer QToolBar QToolButton:hover {
  background-color: #5c5c5c;
}
.button-flat:pressed,
PaletteViewer QToolBar QToolButton:pressed {
  background-color: #292929;
}
.button-radio:unchecked,
#CameraSettingsRadioButton_Small::indicator:unchecked {
  image: url('imgs/white/radiobutton_unchecked.png');
}
.button-radio:checked,
#CameraSettingsRadioButton_Small::indicator:checked {
  image: url('imgs/white/radiobutton_checked.png');
}
/* -----------------------------------------------------------------------------
   Component: Group Boxes
----------------------------------------------------------------------------- */
.box-base,
QGroupBox,
#LoadLevelFrame,
#PsdSettingsGroupBox,
#CleanupSettingsFrame,
#OutputSettingsBox,
#OutputSettingsCameraBox,
#SolidLineFrame,
#FunctionParametersPanel {
  border: 1px solid #626262;
  border-radius: 2px;
}
/* -----------------------------------------------------------------------------
   Component: Icons
----------------------------------------------------------------------------- */
.icon-lock,
#CameraSettingsRadioButton::indicator,
#EditToolLockButton::indicator {
  height: 16px;
  padding: 3px;
  width: 16px;
}
.icon-lock:unchecked,
#CameraSettingsRadioButton::indicator:unchecked,
#EditToolLockButton::indicator:unchecked {
  image: url('imgs/white/cam_unlock.png');
}
.icon-lock:checked,
#CameraSettingsRadioButton::indicator:checked,
#EditToolLockButton::indicator:checked {
  image: url('imgs/white/cam_lock.png');
}
.icon-fsp,
#ForceSquaredPixelButton {
  height: 16px;
  image: url('imgs/white/fsp_unchecked.png');
  padding: 1px;
  width: 16px;
}
.icon-fsp:checked,
#ForceSquaredPixelButton:checked {
  image: url('imgs/white/fsp_checked.png');
}
.icon-fsp:checked:hover,
#ForceSquaredPixelButton:checked:hover {
  background-color: #343434;
}
/* -----------------------------------------------------------------------------
   Component: Tabs
----------------------------------------------------------------------------- */
.tab-container,
#TabBarContainer,
#StyleEditorTabBar,
#PaletteTabBar,
#FxSettingsTabBar {
  background-color: transparent;
  qproperty-BottomAboveLineColor: #343434;
  qproperty-BottomBelowLineColor: #242424;
}
.tab-flat,
#StyleEditorTabBar::tab,
#PaletteTabBar::tab {
  background-color: #343434;
  border-right: 1px solid #222222;
  color: #999999;
  margin: 0 0 1px 0;
  padding: 3px 4px 3px 4px;
}
.tab-flat:hover,
#StyleEditorTabBar::tab:hover,
#PaletteTabBar::tab:hover {
  background-color: #404040;
}
.tab-flat:selected,
#StyleEditorTabBar::tab:selected,
#PaletteTabBar::tab:selected {
  background-color: #484848;
  color: #e5e5e5;
  margin: 0;
  padding: 1px 4px 2px 4px;
}
.tab-flat:only-one,
#StyleEditorTabBar::tab:only-one,
#PaletteTabBar::tab:only-one {
  margin: 0;
  padding: 3px 4px 4px 4px;
}
.tab-round,
#TopBarTab::tab,
#FxSettingsTabBar::tab {
  background-color: #343434;
  border-top: 1px solid #222222;
  border-right: 1px solid #222222;
  border-left: 1px solid #222222;
  color: #999999;
  margin: 3px -1px 1px 0;
  padding: 2px 7px 1px 7px;
}
.tab-round:selected,
#TopBarTab::tab:selected,
#FxSettingsTabBar::tab:selected {
  background-color: #484848;
  border-top-right-radius: 2px;
  border-top-left-radius: 2px;
  color: #e5e5e5;
  margin: 1px -1px 0 0;
  padding: 2px 7px 2px 7px;
}
.tab-round:only-one,
#TopBarTab::tab:only-one,
#FxSettingsTabBar::tab:only-one {
  margin: 1px 0 0 0;
  padding: 3px 7px 3px 7px;
}
.tab-round:last,
#TopBarTab::tab:last,
#FxSettingsTabBar::tab:last {
  margin-right: 0;
  border-top-right-radius: 2px;
}
.tab-round:first,
#TopBarTab::tab:first,
#FxSettingsTabBar::tab:first {
  border-top-left-radius: 2px;
}
