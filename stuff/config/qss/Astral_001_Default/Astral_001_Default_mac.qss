/* -----------------------------------------------------------------------------
   Main
----------------------------------------------------------------------------- */
QWidget {
  background-color: #484848;
  color: #bfbfbf;
}
QWidget:disabled {
  color: #737373;
}
QFrame {
  border: 0;
  margin: 0;
  padding: 0;
}
/* -----------------------------------------------------------------------------
   Tooltip
----------------------------------------------------------------------------- */
QToolTip,
#helpToolTip {
  background-color: #ffffe1;
  border: 1px solid #000;
  color: #000;
  padding: 1px 0;
}
/* -----------------------------------------------------------------------------
   Dock Separator
----------------------------------------------------------------------------- */
#DockSeparator,
QMainWindow::separator,
QSplitter::handle {
  background-color: #222222;
  height: 4px;
  width: 4px;
}
#TDockPlaceholder {
  background-color: #c6ff00;
}
/* -----------------------------------------------------------------------------
   TopBar / QMenuBar
----------------------------------------------------------------------------- */
#TopBar {
  border-bottom: 1px solid #292929;
}
#TopBar #EditToolLockButton {
  spacing: 0;
}
#TopBar #EditToolLockButton::indicator {
  background-color: transparent;
  border: 0;
  border-radius: 0;
  margin: 0;
  padding: 2px 3px 0 2px;
}
#TopBarTabContainer {
  background-color: #484848;
  margin-bottom: 1px;
}
#StackedMenuBar {
  border: 0;
  margin: 0;
  padding: 0;
}
QMenuBar {
  background-color: #484848;
}
QMenuBar::item {
  background-color: #484848;
  border-left: 1px solid #484848;
  margin: 0;
  padding: 3px 5px;
}
QMenuBar::item:selected {
  background-color: #5c5c5c;
  color: #bfbfbf;
}
QMenuBar::item:pressed {
  background-color: #80a0dc;
  color: #000;
}
/* -----------------------------------------------------------------------------
   Room Tabs
----------------------------------------------------------------------------- */
#TopBarTab {
  margin: 0;
  padding: 0;
}
#TopBarTab::tab {
  background-color: #343434;
  border-color: #222222;
  color: #999999;
  padding-top: 1px;
  padding-bottom: 1px;
  margin-bottom: 0;
}
#TopBarTab::tab:selected {
  background-color: #484848;
  color: #e5e5e5;
  padding: 0 7px 0 7px;
}
/* -----------------------------------------------------------------------------
   QMenu (Context Menu)
----------------------------------------------------------------------------- */
QMenu {
  background-color: #3b3b3b;
  border: 1px solid #292929;
  padding: 2px 0;
}
QMenu::item {
  padding: 3px 28px;
}
QMenu::item:selected {
  background-color: #80a0dc;
  color: #000;
}
QMenu::item:checked {
  color: #bfbfbf;
}
QMenu::item:checked:selected {
  background-color: #80a0dc;
  color: #000;
}
QMenu::item:disabled {
  background: none;
  color: #737373;
}
QMenu::separator {
  border-top: 1px solid #292929;
  height: 0;
  margin: 3px 0;
}
QMenu::icon {
  border-radius: 2px;
  margin: 0 0 0 3px;
  padding: 1px;
}
QMenu::icon:checked {
  background-color: #80a0dc;
}
QMenu::indicator {
  margin-left: 7px;
}
/* -----------------------------------------------------------------------------
   Title Bars (Global)
----------------------------------------------------------------------------- */
TPanelTitleBar {
  background-color: #3b3b3b;
  border-bottom: 1px solid #292929;
  height: 20px;
  min-height: 20px;
  qproperty-TitleColor: #a6a6a6;
  qproperty-ActiveTitleColor: #d9d9d9;
  qproperty-BorderPixmap: url('imgs/white/none');
  qproperty-ActiveBorderPixmap: url('imgs/white/none');
  qproperty-FloatBorderPixmap: url('imgs/white/none');
  qproperty-FloatActiveBorderPixmap: url('imgs/white/none');
}
TPanel {
  background-color: #222222;
}
/* -----------------------------------------------------------------------------
   Scroll Bars (Global)
----------------------------------------------------------------------------- */
QAbstractScrollArea::corner {
  background-color: #2f2f2f;
}
QScrollBar {
  background-color: #2f2f2f;
  border: 0;
}
QScrollBar:horizontal {
  height: 17px;
  margin: 0;
}
QScrollBar:vertical {
  margin: 0;
  width: 17px;
}
QScrollBar::handle {
  background-color: #484848;
  border: 1px solid #4b4b4b;
}
QScrollBar::handle:horizontal {
  margin: 1px 17px;
  min-width: 20px;
}
QScrollBar::handle:vertical {
  margin: 17px 1px;
  min-height: 20px;
}
QScrollBar::add-line {
  subcontrol-origin: margin;
  background-color: #484848;
  border: 1px solid #484848;
}
QScrollBar::add-line:horizontal:pressed,
QScrollBar::add-line:vertical:pressed {
  background-color: #555555;
  border-color: #555555;
}
QScrollBar::add-line:horizontal {
  subcontrol-position: right;
  image: url('imgs/white/scroll-right.png');
  image-position: top;
  margin: 1px;
  width: 13px;
}
QScrollBar::add-line:vertical {
  subcontrol-position: bottom;
  image: url('imgs/white/scroll-down.png');
  image-position: left;
  margin: 1px;
  height: 13px;
}
QScrollBar::sub-line {
  background-color: #484848;
  border: 1px solid #484848;
  subcontrol-origin: margin;
}
QScrollBar::sub-line:horizontal:pressed,
QScrollBar::sub-line:vertical:pressed {
  background-color: #555555;
  border-color: #555555;
}
QScrollBar::sub-line:horizontal {
  subcontrol-position: left;
  image: url('imgs/white/scroll-left.png');
  image-position: top;
  margin: 1px;
  width: 13px;
}
QScrollBar::sub-line:vertical {
  subcontrol-position: top;
  image: url('imgs/white/scroll-up.png');
  image-position: top;
  margin: 1px;
  height: 13px;
}
QScrollBar::sub-page:horizontal,
QScrollBar::add-page:horizontal,
QScrollBar::sub-page:vertical,
QScrollBar::add-page:vertical {
  background: none;
}
/* -----------------------------------------------------------------------------
   Tool Bars (Global)
----------------------------------------------------------------------------- */
QToolBar {
  padding: 0;
}
QToolBar::separator:horizontal {
  border-left: 1px solid #292929;
  margin: 0 1px;
  width: 0;
}
QToolBar::separator:vertical {
  border-top: 1px solid #292929;
  height: 0;
  margin: 1px 0;
}
/* -------------------------------------------------------------------------- */
DvScrollWidget QPushButton,
DvScrollWidget #WordButton,
DvScrollWidget #FunctionSegmentViewerLinkButton {
  background-color: #e1e1e1;
  border: 0;
  border-radius: 1px;
  padding: 0;
  max-width: 15px;
  min-width: 15px;
}
DvScrollWidget QPushButton:hover,
DvScrollWidget #WordButton:hover,
DvScrollWidget #FunctionSegmentViewerLinkButton:hover {
  background-color: #f5f5f5;
}
DvScrollWidget QPushButton:pressed,
DvScrollWidget #WordButton:pressed,
DvScrollWidget #FunctionSegmentViewerLinkButton:pressed {
  background-color: #c8c8c8;
}
#ScrollLeftButton,
#ScrollRightButton,
#ScrollUpButton,
#ScrollDownButton {
  max-width: 15px;
  min-width: 15px;
}
#ScrollLeftButton {
  image: url('imgs/white/arrow_left.png');
}
#ScrollRightButton {
  image: url('imgs/white/arrow_right.png');
}
#ScrollUpButton {
  image: url('imgs/white/arrow_up.png');
}
#ScrollDownButton {
  image: url('imgs/white/arrow_down.png');
}
#keyFrameNavigator {
  background: none;
  margin: 0;
  padding: 0;
}
#keyFrameNavigator QToolButton {
  min-width: 18px;
}
/* -----------------------------------------------------------------------------
   Treeviews (Global)
----------------------------------------------------------------------------- */
QListView {
  outline: 0;
}
/* -----------------------------------------------------------------------------
   Tabs (Global)
----------------------------------------------------------------------------- */
#TabBarContainer {
  background-color: #343434;
}
/* -----------------------------------------------------------------------------
   UI Controls
----------------------------------------------------------------------------- */
QPushButton,
QComboBox,
#WordButton,
#FunctionSegmentViewerLinkButton {
  background-color: #5c5c5c;
  border: 1px solid #343434;
  border-radius: 2px;
  color: #cccccc;
  padding: 3px 15px;
}
QPushButton:hover,
QComboBox:hover,
#WordButton:hover,
#FunctionSegmentViewerLinkButton:hover {
  background-color: #696969;
  border-color: #343434;
  color: #d9d9d9;
}
QPushButton:pressed,
QComboBox:checked,
#WordButton:pressed,
#FunctionSegmentViewerLinkButton:pressed {
  background-color: #343434;
  border-color: #1a1a1a;
  color: #bfbfbf;
}
QPushButton:checked,
#WordButton:checked,
#FunctionSegmentViewerLinkButton:checked {
  background-color: #272727;
  border-color: #0d0d0d;
  color: #bfbfbf;
}
QPushButton:checked:hover,
#WordButton:checked:hover,
#FunctionSegmentViewerLinkButton:checked:hover {
  background-color: #343434;
}
QPushButton:disabled,
QComboBox:disabled,
#WordButton:disabled,
#FunctionSegmentViewerLinkButton:disabled {
  background-color: #525252;
  border-color: #3e3e3e;
  color: #737373;
}
#PushButton_NoPadding {
  padding: 3px;
}
/* Camera Capture */
#GearButton {
  qproperty-icon: url('imgs/white/gear.png');
}
#SubfolderButton {
  qproperty-icon: url('imgs/white/subfolder.png');
}
/* -------------------------------------------------------------------------- */
QComboBox {
  padding: 1px 5px;
}
QComboBox::drop-down {
  border: 0;
}
QComboBox::down-arrow {
  image: url('imgs/white/combo_down_arrow.png');
}
QComboBox::down-arrow:disabled {
  image: url('imgs/white/combo_down_arrow-disabled.png');
}
QComboBox QAbstractItemView {
  background-color: #3b3b3b;
  border: 1px solid #292929;
  selection-background-color: #80a0dc;
  selection-color: #000;
}
/* -------------------------------------------------------------------------- */
QLineEdit,
#TaskSheetItem,
#tasksRemoveBox,
#tasksAddBox {
  background-image: url('imgs/white/lineedit_shadow.png');
  background-repeat: repeat-x;
  background-color: #2f2f2f;
  border: 1px solid #272727;
  border-radius: 2px;
  color: #bfbfbf;
  padding: 0 2px;
  selection-background-color: #80a0dc;
  selection-color: #000;
}
QLineEdit:focus,
#TaskSheetItem:focus,
#tasksRemoveBox:focus,
#tasksAddBox:focus {
  border-color: #5c7ab4;
}
QLineEdit:disabled,
#TaskSheetItem:disabled,
#tasksRemoveBox:disabled,
#tasksAddBox:disabled {
  background-color: #3b3b3b;
  border-color: #2f2f2f;
  color: #737373;
}
/* -------------------------------------------------------------------------- */
QCheckBox {
  color: #bfbfbf;
}
QCheckBox:hover {
  color: #d9d9d9;
}
QCheckBox:disabled {
  color: #737373;
}
QCheckBox::indicator,
QMenu::indicator,
QGroupBox::indicator {
  background-color: #2f2f2f;
  border: 1px solid #272727;
  border-radius: 2px;
  height: 9px;
  padding: 1px;
  width: 9px;
}
QCheckBox::indicator:hover,
QMenu::indicator:hover,
QCheckBox::indicator:checked:hover,
QGroupBox::indicator:hover,
QMenu::indicator:checked:hover,
